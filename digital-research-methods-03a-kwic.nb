Cell[CellGroupData[{

Cell["Another search function  ", "Section",
 CellChangeTimes->{{3.647612524756513*^9, 3.647612569658021*^9}}],

Cell["We wrap everything up in a new function so we can reuse it.", "Text",
 CellChangeTimes->{{3.647612571730135*^9, 3.6476125985537653`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"searchNearWordList", "[", 
   RowBox[{"wlist_", ",", "kw1_", ",", "kw2_", ",", "within_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"TabView", "[", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Column", "@", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"StringRiffle", "@", 
          RowBox[{"wlist", "\[LeftDoubleBracket]", 
           RowBox[{
            RowBox[{"Max", "[", 
             RowBox[{"1", ",", 
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
               "-", "10"}]}], "]"}], ";;", 
            RowBox[{"Max", "[", 
             RowBox[{"1", ",", 
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
               "-", "1"}]}], "]"}]}], "\[RightDoubleBracket]"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Style", "[", 
          RowBox[{
           RowBox[{"StringRiffle", "@", 
            RowBox[{"wlist", "\[LeftDoubleBracket]", 
             RowBox[{
              RowBox[{
              "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], ";;", 
              RowBox[{
              "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
             "\[RightDoubleBracket]"}]}], ",", "Bold"}], "]"}], ",", 
         RowBox[{"StringRiffle", "@", 
          RowBox[{"wlist", "\[LeftDoubleBracket]", 
           RowBox[{
            RowBox[{"Min", "[", 
             RowBox[{
              RowBox[{"Length", "[", "wlist", "]"}], ",", 
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
               "+", "1"}]}], "]"}], ";;", 
            RowBox[{"Min", "[", 
             RowBox[{
              RowBox[{"Length", "[", "wlist", "]"}], ",", 
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
               "+", "10"}]}], "]"}]}], "\[RightDoubleBracket]"}]}]}], "}"}]}],
       "&"}], ",", 
     RowBox[{"Map", "[", 
      RowBox[{"Sort", ",", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"Tuples", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Flatten", "[", 
             RowBox[{"Position", "[", 
              RowBox[{"wlist", ",", "kw1"}], "]"}], "]"}], ",", 
            RowBox[{"Flatten", "[", 
             RowBox[{"Position", "[", 
              RowBox[{"wlist", ",", "kw2"}], "]"}], "]"}]}], "}"}], "]"}], 
         ",", 
         RowBox[{
          RowBox[{
           RowBox[{"Abs", "[", 
            RowBox[{
             RowBox[{
             "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "-", 
             RowBox[{
             "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
            "]"}], "<", "within"}], "&"}]}], "]"}]}], "]"}]}], "]"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.6476106257044783`*^9, 3.64761066203255*^9}, {
  3.647610711711185*^9, 3.6476108118593407`*^9}, {3.647617377692828*^9, 
  3.647617379193931*^9}}],

Cell[TextData[{
 "The ",
 StyleBox["TabView",
  FontWeight->"Bold"],
 " command lets us tab through a set of results. Here are the places in the \
text where \[OpenCurlyDoubleQuote]mad\[CloseCurlyDoubleQuote] is within 30 \
words of \[OpenCurlyDoubleQuote]hatter\[CloseCurlyDoubleQuote]."
}], "Text",
 CellChangeTimes->{{3.647612603401103*^9, 3.64761263779214*^9}}],

Cell[BoxData[
 RowBox[{"searchNearWordList", "[", 
  RowBox[{
  "aliceWordlist", ",", "\"\<mad\>\"", ",", "\"\<hatter\>\"", ",", "30"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.64761082062006*^9, 3.6476108363380833`*^9}}],

Cell[TextData[{
 "All the places in ",
 StyleBox["Alice in Wonderland",
  FontSlant->"Italic"],
 " where \[OpenCurlyDoubleQuote]march\[CloseCurlyDoubleQuote] appears near \
\[OpenCurlyDoubleQuote]hare\[CloseCurlyDoubleQuote]."
}], "Text",
 CellChangeTimes->{{3.647612640695746*^9, 3.647612658735409*^9}, {
  3.647613737065215*^9, 3.6476137475195827`*^9}}],

Cell[BoxData[
 RowBox[{"searchNearWordList", "[", 
  RowBox[{
  "aliceWordlist", ",", "\"\<march\>\"", ",", "\"\<hare\>\"", ",", "30"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.64761082062006*^9, 3.647610852929576*^9}}]
