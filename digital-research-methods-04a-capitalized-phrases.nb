 CellChangeTimes->{{3.647662962534761*^9, 3.647662964206275*^9}, {
  3.647663240924993*^9, 3.647663246411808*^9}}],

Cell[TextData[{
 "We can take the first character of a string with ",
 StyleBox["StringTake",
  FontWeight->"Bold"],
 ", and test to see if it is uppercase with ",
 StyleBox["UpperCaseQ",
  FontWeight->"Bold"],
 "."
}], "Text",
 CellChangeTimes->{{3.639673669251041*^9, 3.6396737632872458`*^9}, {
  3.639674316971384*^9, 3.639674362722455*^9}, {3.640883362279398*^9, 
  3.6408833913808117`*^9}, {3.6476632667320127`*^9, 3.647663267371139*^9}}],

Cell[BoxData[
 RowBox[{"StringTake", "[", 
  RowBox[{"\"\<John\>\"", ",", "1"}], "]"}]], "Input",
 CellChangeTimes->{{3.639673711695339*^9, 3.6396737182977743`*^9}, {
  3.64766327450765*^9, 3.647663275018921*^9}}],

Cell[BoxData[
 RowBox[{"UpperCaseQ", "[", 
  RowBox[{"StringTake", "[", 
   RowBox[{"\"\<John\>\"", ",", "1"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.639673711695339*^9, 3.6396737182977743`*^9}, {
  3.639673756816893*^9, 3.6396737656395483`*^9}, {3.647663279498478*^9, 
  3.647663280090745*^9}}],

Cell[TextData[{
 "Let\[CloseCurlyQuote]s create a small function that tests to see if a word \
is capitalized. In keeping with the ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " convention, we name this function so it ends in a capital Q."
}], "Text",
 CellChangeTimes->{{3.639674165873502*^9, 3.63967418598457*^9}, {
   3.63967427126473*^9, 3.6396743102518787`*^9}, 3.639674369593606*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"capitalizedQ", "[", "w_", "]"}], ":=", 
  RowBox[{"UpperCaseQ", "[", 
   RowBox[{"StringTake", "[", 
    RowBox[{"w", ",", "1"}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.639674194783305*^9, 3.639674210052472*^9}}],

Cell[BoxData[
 RowBox[{"capitalizedQ", "[", "\"\<John\>\"", "]"}]], "Input",
 CellChangeTimes->{{3.639674214389905*^9, 3.6396742183441887`*^9}, {
  3.647663292491003*^9, 3.6476632938821917`*^9}}],

