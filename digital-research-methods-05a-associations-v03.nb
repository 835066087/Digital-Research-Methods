ca54f687d2df"],

Cell[BoxData[
 RowBox[{
  RowBox[{"theXofYZ", "=", 
   RowBox[{"GroupBy", "[", 
    RowBox[{
     RowBox[{"Cases", "[", 
      RowBox[{
       RowBox[{"Partition", "[", 
        RowBox[{
         RowBox[{"TextWords", "[", 
          RowBox[{"ToLowerCase", "[", "lippmanText", "]"}], "]"}], ",", "5", 
         ",", "1"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{
        "\"\<the\>\"", ",", "_", ",", "\"\<of\>\"", ",", "_", ",", "_"}], 
        "}"}]}], "]"}], ",", 
     RowBox[{
      RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
      "&"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6479610717374153`*^9, 3.647961105178419*^9}, {
  3.647961184734572*^9, 3.647961250957891*^9}, {3.647961291611717*^9, 
  3.647961311923016*^9}, {3.6479617586360397`*^9, 3.647961774762141*^9}, {
  3.647961949284032*^9, 3.647961974226963*^9}, {3.6481010324208384`*^9, 
  3.648101039451137*^9}, {3.651400729969883*^9, 3.651400737638506*^9}, {
  3.7349536511156816`*^9, 3.7349536738257236`*^9}, {3.7349537279362917`*^9, 
  3.7349537812306223`*^9}},
 CellLabel->"In[59]:=",ExpressionUUID->"4cbc0cdf-fce1-439f-b9c0-4251576273ac"],

