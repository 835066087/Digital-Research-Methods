Cell[BoxData[
 RowBox[{"Short", "[", 
  RowBox[{"gettysburg", ",", "10"}], "]"}]], "Input",
 CellChangeTimes->{{3.647354317001151*^9, 3.647354326689513*^9}}]
}, Open  ]]
}, Open  ]],

