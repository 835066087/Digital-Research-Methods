Cell[CellGroupData[{

Cell["Gettysburg Address", "Section",
 CellChangeTimes->{{3.6473541406328173`*^9, 3.6473541477599688`*^9}}],

Cell[BoxData[
 RowBox[{"ExampleData", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\"\<Text\>\"", ",", "\"\<GettysburgAddress\>\""}], "}"}], ",", 
   "\"\<Author\>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.647353901640874*^9, 3.647353933360815*^9}, {
  3.6473549704506617`*^9, 3.647355014944206*^9}}],

Cell[BoxData[
 RowBox[{"ExampleData", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\"\<Text\>\"", ",", "\"\<GettysburgAddress\>\""}], "}"}], ",", 
   "\"\<FullTitle\>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.647353901640874*^9, 3.647353933360815*^9}, {
  3.6473549704506617`*^9, 3.647355023759922*^9}}],

Cell[TextData[{
 "We use an equals sign to assign a ",
 StyleBox["value",
  FontSlant->"Italic"],
 " to a ",
 StyleBox["symbol",
  FontSlant->"Italic"],
 ". We give the symbol a name that begins with a lowercase letter, so it \
doesn\[CloseCurlyQuote]t get confused with a built-in command. If we put a \
semi-colon at the end of a line, the output of that line is suppressed (i.e., \
it is not printed in your ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " notebook)."
}], "Text",
 CellChangeTimes->{{3.6473550418876343`*^9, 3.6473550654302177`*^9}, {
  3.64735511581222*^9, 3.647355162322762*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"gettysburg", "=", 
   RowBox[{"ExampleData", "[", 
    RowBox[{"{", 
     RowBox[{"\"\<Text\>\"", ",", "\"\<GettysburgAddress\>\""}], "}"}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.647354164350943*^9, 3.647354202549961*^9}, {
  3.647354339986651*^9, 3.647354342849024*^9}}],

Cell["The value in this case is a string.", "Text",
 CellChangeTimes->{{3.647355172514224*^9, 3.6473551860737*^9}, 
   3.647356281651051*^9}],

Cell[BoxData[
 RowBox[{"Head", "[", "gettysburg", "]"}]], "Input",
 CellChangeTimes->{{3.6473543496819763`*^9, 3.647354352897361*^9}}],

Cell["Number of characters in the string:", "Text",
 CellChangeTimes->{{3.647355076237814*^9, 3.647355078413727*^9}, {
   3.647355187913603*^9, 3.647355189139472*^9}, 3.647356284929461*^9}],

Cell[BoxData[
 RowBox[{"StringLength", "[", "gettysburg", "]"}]], "Input",
 CellChangeTimes->{{3.647354354186245*^9, 3.647354359504299*^9}}],

Cell["Show a small amount of data (about 10 lines in this case)", "Text",
