  RowBox[{"<", 
   RowBox[{"a", " ", "href"}]}], "=", 
  RowBox[{"\"\<http://www.darwinproject.ac.uk\>\"", ">", 
   RowBox[{"Darwin", " ", "Correspondence", " ", "Project"}], "<", 
   RowBox[{"/", "a"}], ">"}]}]], "DisplayFormula",
