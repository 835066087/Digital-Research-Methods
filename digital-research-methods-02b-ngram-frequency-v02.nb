      RowBox[{"Flatten", "[", 
       RowBox[{"StringCases", "[", 
        RowBox[{"gwbFeb2001TermList", ",", 
         RowBox[{"__", "~~", "\"\<ed\>\"", "~~", "WordBoundary"}]}], "]"}], 
       "]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"\"\<hundred\>\"", ",", "\"\<need\>\""}], "}"}]}], "]"}]}], 
  ";"}]], "Input",
