   RowBox[{"Complement", "[", 
    RowBox[{
     RowBox[{"Union", "[", 
      RowBox[{"Flatten", "[", 
       RowBox[{"StringCases", "[", 
        RowBox[{"gwbSep2001TermList", ",", 
         RowBox[{"__", "~~", "\"\<ed\>\"", "~~", "WordBoundary"}]}], "]"}], 
       "]"}], "]"}], ",", 
     RowBox[{"{", "\"\<need\>\"", "}"}]}], "]"}]}], ";"}]], "Input",
