Cell[CellGroupData[{

Cell["Word clouds", "Section",
 CellChangeTimes->{{3.6473429438950644`*^9, 3.647342944950231*^9}, {
  3.647353742456744*^9, 3.647353748014737*^9}, {3.681296708530692*^9, 
  3.6812967094017897`*^9}, {3.681309732030527*^9, 3.6813097336696157`*^9}}],

Cell[TextData[{
 "In ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ", we can use the ",
 StyleBox["WordCloud",
  FontWeight->"Bold"],
 " command to create a display which scales each word in a text according to \
its frequency. Here we begin with the string containing the text of the \
speech, ",
 StyleBox["DeleteStopwords",
  FontWeight->"Bold"],
 ", and then create the word cloud."
}], "Text",
 CellChangeTimes->{{3.681309744519806*^9, 3.681309746903287*^9}, {
  3.681309789699951*^9, 3.6813097914035788`*^9}, {3.681309867217222*^9, 
  3.681309891591721*^9}}],

Cell[CellGroupData[{

Cell["February 2001", "Subsection",
 CellChangeTimes->{{3.681309755781232*^9, 3.6813097589565763`*^9}}],

Cell[BoxData[
 RowBox[{"WordCloud", "[", 
  RowBox[{"DeleteStopwords", "[", "gwbFeb2001String", "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.6813097671914663`*^9, 3.68130977852757*^9}, {
  3.6813098243106422`*^9, 3.681309860585894*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["September 2001", "Subsection",
 CellChangeTimes->{{3.681309762980336*^9, 3.681309764916553*^9}}],

Cell[BoxData[
 RowBox[{"WordCloud", "[", 
  RowBox[{"DeleteStopwords", "[", "gwbSep2001String", "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.6813097671914663`*^9, 3.68130977852757*^9}, {
  3.6813098243106422`*^9, 3.681309860585894*^9}, {3.6813099022884703`*^9, 
  3.68130990272788*^9}}]
}, Open  ]]
}, Open  ]]
}, Open  ]],

