Cell["\<\
Suppose we wanted to find the events relating to the early 1970s. First we \
download the entity, name and start date for each event in the form of an \
association. This takes a while.\
\>", "Text",
 CellChangeTimes->{{3.651757116235545*^9, 3.65175717693676*^9}, {
  3.651757335124186*^9, 3.6517573356432858`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"histevents", "=", 
   RowBox[{"EntityValue", "[", 
    RowBox[{
     RowBox[{"EntityValue", "[", 
      RowBox[{"\"\<HistoricalEvent\>\"", ",", "\"\<Entities\>\""}], "]"}], 
     ",", 
     RowBox[{"{", 
      RowBox[{"\"\<Name\>\"", ",", "\"\<StartDate\>\""}], "}"}], ",", 
     "\"\<EntityAssociation\>\""}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.641755046859334*^9, 3.6417551745854816`*^9}, {
  3.6517594973373632`*^9, 3.651759498199438*^9}}],

Cell[TextData[{
 "Next we ",
 StyleBox["Select",
  FontWeight->"Bold"],
 " those events whose starting date is between 1970 and 1971"
}], "Text",
 CellChangeTimes->{{3.651757213680064*^9, 3.6517572256631527`*^9}, {
   3.65175735460251*^9, 3.651757372450098*^9}, 3.6517574590797863`*^9}],

Cell[BoxData[
 RowBox[{"viewData", "[", 
  RowBox[{"Keys", "[", 
   RowBox[{"Select", "[", 
    RowBox[{"histevents", ",", 
     RowBox[{
      RowBox[{"1970", "\[LessEqual]", 
       RowBox[{
        RowBox[{"DateList", "[", 
         RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
         "]"}], "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
       "\[LessEqual]", "1971"}], "&"}]}], "]"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.65175723717719*^9, 3.651757305485091*^9}, 
   3.6517573438513527`*^9, 3.651757462855201*^9, {3.651758225837451*^9, 
   3.651758230077169*^9}, {3.6517582773075123`*^9, 3.651758282419322*^9}}],

