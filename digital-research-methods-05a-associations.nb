       RowBox[{"Thread", "[", 
        RowBox[{"List", "[", 
         RowBox[{"idxl", ",", "left"}], "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rightidx", "=", 
      RowBox[{"StringRiffle", "/@", 
       RowBox[{"Thread", "[", 
        RowBox[{"List", "[", 
