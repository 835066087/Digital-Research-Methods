          RowBox[{
           RowBox[{
            RowBox[{"StringJoin", "[", 
             RowBox[{"#", ",", "\"\< \>\""}], "]"}], "&"}], "/@", "right"}]}],
          "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"lself", "=", 
      RowBox[{"Thread", "[", 
       RowBox[{"Rule", "[", 
        RowBox[{"leftidx", ",", "leftidx"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rself", "=", 
      RowBox[{"Thread", "[", 
       RowBox[{"Rule", "[", 
        RowBox[{"rightidx", ",", "rightidx"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"lcoords", "=", 
      RowBox[{"Thread", "[", 
       RowBox[{"Rule", "[", 
