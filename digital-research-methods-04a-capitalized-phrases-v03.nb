Cell[CellGroupData[{

Cell["Removing sentential capitalization", "Section",
 CellChangeTimes->{{3.647361153919208*^9, 3.6473611548467712`*^9}, {
  3.7346343047200937`*^9, 3.734634307825851*^9}, {3.734708572560671*^9, 
  3.7347086021507063`*^9}},ExpressionUUID->"ae1e1fd7-f524-42c3-9b2b-\
218aaea0c83f"],

Cell["\<\
Note that some of these words are only capitalized because they are the first \
word in a sentence. If we want to eliminate these from our list of \
capitalized words, we can go through our list of mixed case sentences and \
convert the first character of each to lower case.\
\>", "Text",
 CellChangeTimes->{{3.734708509594796*^9, 3.734708533841297*^9}, {
  3.734708609518358*^9, 3.734708632413741*^9}, {3.7347086635804768`*^9, 
  3.734708680995973*^9}, {3.7347087801444263`*^9, 
  3.734708795015821*^9}},ExpressionUUID->"36e31852-8198-4fd2-9e89-\
6f8255b5a369"],

Cell[BoxData[
 RowBox[{
  RowBox[{"schreckerInitialLCSentences", "=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"StringLength", "[", "#", "]"}], ">", "0"}], ",", 
        RowBox[{"StringJoin", "[", 
         RowBox[{
          RowBox[{"ToLowerCase", "[", 
           RowBox[{"StringTake", "[", 
            RowBox[{"#", ",", "1"}], "]"}], "]"}], ",", 
          RowBox[{"StringDrop", "[", 
           RowBox[{"#", ",", "1"}], "]"}]}], "]"}]}], "]"}], "&"}], ",", 
     "schreckerMixedCaseSentences"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.734708815910808*^9, 3.734708836943191*^9}, {
  3.7347089149721622`*^9, 3.734708968970229*^9}, {3.734709206152629*^9, 
  3.734709211840534*^9}, {3.734709277232273*^9, 3.734709333028117*^9}, {
  3.734709636058652*^9, 3.734709640729649*^9}, {3.7347098396583023`*^9, 
  3.734709840945424*^9}},
 CellLabel->"In[93]:=",ExpressionUUID->"f6a88862-56a3-4782-b194-c99221c36cd4"],

Cell[BoxData[
 RowBox[{"First", "[", "schreckerInitialLCSentences", "]"}]], "Input",
 CellChangeTimes->{{3.7347093342266006`*^9, 3.734709353388035*^9}},
 CellLabel->"In[94]:=",ExpressionUUID->"8a026e11-4825-4f48-b6b0-1d530e3c2cc9"],

Cell["\<\
Next we extract all of the unique words. Any word that is capitalized in this \
list was not in the first position in a sentence.\
\>", "Text",
 CellChangeTimes->{{3.734709362986555*^9, 3.7347093738103437`*^9}, 
   3.734709453432325*^9, {3.7347099603255796`*^9, 
   3.734710003019191*^9}},ExpressionUUID->"2255853a-05c8-4899-a875-\
860e35cf6a75"],

Cell[BoxData[
 RowBox[{
  RowBox[{"schreckerInitialLCWords", "=", 
   RowBox[{"Union", "[", 
    RowBox[{"TextWords", "[", 
     RowBox[{"StringRiffle", "[", "schreckerInitialLCSentences", "]"}], "]"}],
     "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7347093992107277`*^9, 3.734709412793044*^9}, {
