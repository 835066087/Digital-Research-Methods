Cell[CellGroupData[{

Cell[TextData[{
 "The ",
 StyleBox["getFreqVector",
  FontSlant->"Italic"],
 " command"
}], "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.653118818306333*^9, 3.65311882338625*^9}}],

Cell["\<\
Here is a function to automate the process that we just did by hand...\
\>", "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.6531205649554977`*^9, 3.653120576114183*^9}, {3.6856346634905453`*^9, 
  3.685634672353518*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"getFreqVector", "[", 
   RowBox[{"textstr_", ",", "freq_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"ReplacePart", "[", 
   RowBox[{
    RowBox[{"ConstantArray", "[", 
     RowBox[{"0", ",", 
      RowBox[{"{", 
       RowBox[{"Length", "[", "freq", "]"}], "}"}]}], "]"}], ",", 
    RowBox[{"Map", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"First", "@", 
          RowBox[{"First", "@", 
           RowBox[{"Position", "[", 
            RowBox[{"freq", ",", "#"}], "]"}]}]}], "\[Rule]", "1"}], ")"}], 
       "&"}], ",", 
      RowBox[{"Intersection", "[", 
       RowBox[{
        RowBox[{"DeleteStopwords", "@", 
         RowBox[{"ToLowerCase", "@", 
          RowBox[{"TextWords", "[", "textstr", "]"}]}]}], ",", "freq"}], 
       "]"}]}], "]"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6428683304516993`*^9, 3.6428683526866817`*^9}, 
   3.64286838924129*^9}],

Cell[TextData[{
 "It allows us to generate a document vector representation, given a page and \
a list of frequent terms. Here is the vector for the first page of ",
 StyleBox["Alice",
  FontSlant->"Italic"],
 " (the one we were computing by hand in the previous slide)."
}], "Text",
 CellChangeTimes->{{3.65312058148987*^9, 3.6531206417199097`*^9}}],

Cell[BoxData[
 RowBox[{"aliceLines", "\[LeftDoubleBracket]", "3", 
  "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.653120717493121*^9, 3.6531207273408737`*^9}}],

Cell[BoxData[
 RowBox[{"getFreqVector", "[", 
  RowBox[{
   RowBox[{
   "aliceLines", "\[LeftDoubleBracket]", "3", "\[RightDoubleBracket]"}], ",", 
   "aliceFrequentTerms"}], "]"}]], "Input",
 CellChangeTimes->{{3.6531204318824463`*^9, 3.653120468135075*^9}, {
  3.653120519773683*^9, 3.653120546851612*^9}}],

Cell["Here is the second page of the book", "Text",
 CellChangeTimes->{{3.653120676279017*^9, 3.653120681590433*^9}}],

Cell[BoxData[
 RowBox[{"aliceLines", "\[LeftDoubleBracket]", "5", 
  "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{3.6531206891188297`*^9}],

Cell["And here is the vector for the second page", "Text",
 CellChangeTimes->{{3.653120693773704*^9, 3.653120699181716*^9}}],

Cell[BoxData[
 RowBox[{"getFreqVector", "[", 
  RowBox[{
   RowBox[{
   "aliceLines", "\[LeftDoubleBracket]", "5", "\[RightDoubleBracket]"}], ",", 
   "aliceFrequentTerms"}], "]"}]], "Input",
 CellChangeTimes->{{3.6531204318824463`*^9, 3.653120468135075*^9}, {
   3.653120519773683*^9, 3.653120546851612*^9}, 3.6531207059103327`*^9}],

Cell["\<\
We can compare the two vectors by eye, and see that they \
don\[CloseCurlyQuote]t overlap.\
\>", "Text",
 CellChangeTimes->{{3.653120753996232*^9, 3.65312076855519*^9}, {
  3.653120799706731*^9, 3.653120833736816*^9}}],

Cell[BoxData[
 RowBox[{"ArrayPlot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"getFreqVector", "[", 
      RowBox[{
       RowBox[{
       "aliceLines", "\[LeftDoubleBracket]", "3", "\[RightDoubleBracket]"}], 
       ",", "aliceFrequentTerms"}], "]"}], ",", 
     RowBox[{"getFreqVector", "[", 
      RowBox[{
       RowBox[{
       "aliceLines", "\[LeftDoubleBracket]", "5", "\[RightDoubleBracket]"}], 
       ",", "aliceFrequentTerms"}], "]"}]}], "}"}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "Full"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.65312084368944*^9, 3.65312088509512*^9}}],

Cell["But, of course, we don't need to compare them by eye.", "Text",
 CellChangeTimes->{{3.653120753996232*^9, 3.65312076855519*^9}, {
  3.653120799706731*^9, 3.65312082169007*^9}, {3.6531209017109737`*^9, 
  3.653120904702016*^9}}]
}, Open  ]]
}, Open  ]],

