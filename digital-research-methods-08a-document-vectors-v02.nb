Cell["Most frequent terms", "Subsubsection",
 CellChangeTimes->{{3.653123140109247*^9, 3.653123143228745*^9}}],

Cell["Here are the terms that appear five or more times", "Text",
 CellChangeTimes->{{3.6531234768082523`*^9, 3.6531234846721277`*^9}}],

Cell[BoxData[
 RowBox[{"ravenFrequentTerms", "=", 
  RowBox[{
   RowBox[{"Select", "[", 
    RowBox[{
     RowBox[{"Sort", "[", 
      RowBox[{
       RowBox[{"Tally", "[", 
        RowBox[{"DeleteStopwords", "[", 
         RowBox[{"Flatten", "[", 
          RowBox[{"ToLowerCase", "[", 
           RowBox[{"TextWords", "[", "ravenLines", "]"}], "]"}], "]"}], "]"}],
         "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}],
          ">", 
         RowBox[{
         "#2", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
        "&"}]}], "]"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
       ">", "4"}], "&"}]}], "]"}], "\[LeftDoubleBracket]", 
   RowBox[{"All", ",", "1"}], "\[RightDoubleBracket]"}]}]], "Input",
 CellChangeTimes->{{3.65311778682251*^9, 3.653117908123805*^9}, {
   3.653117944763323*^9, 3.653118041774969*^9}, {3.653118089374113*^9, 
