    RowBox[{"getOccurrences", "[", 
     RowBox[{"aliceLineWordLists", ",", 
      RowBox[{"{", 
       RowBox[{"\"\<march\>\"", ",", "\"\<hare\>\""}], "}"}]}], "]"}]}], 
   "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.647751745579569*^9, 3.647751800602219*^9}, {
  3.6477519928601007`*^9, 3.647751993884453*^9}, {3.647788550597053*^9, 
