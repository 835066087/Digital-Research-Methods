      RowBox[{"{", 
       RowBox[{"\"\<Page Number\>\"", ",", "\"\<Brightness\>\""}], "}"}]}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"PlotRange", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"1", ",", 
          RowBox[{"Length", "[", "emotionsThumbs", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "1"}], "}"}]}], "}"}]}]}], "]"}]}], "]"}]], "Input",\

 CellChangeTimes->{{3.535184833537919*^9, 3.535184928558133*^9}, {
