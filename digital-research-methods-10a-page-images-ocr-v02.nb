Cell[BoxData[
 RowBox[{
  RowBox[{"emotionsThumbs", "=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Import", "[", 
       RowBox[{
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{
          "localDownloadFilePath", ",", 
           "\"\<expressionofem00darw_jp2_thumbs.zip\>\""}], "}"}], "]"}], ",",
         "#"}], "]"}], "&"}], ",", 
     RowBox[{"Flatten", "[", 
      RowBox[{"StringCases", "[", 
