Cell[BoxData[
 RowBox[{
  RowBox[{"formatWAPersonData", "[", "assoc_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"version", " ", "20160920"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"Grid", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"assoc", "[", "\"\<Image\>\"", "]"}], ",", 
       RowBox[{"Column", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Text", "@", 
           RowBox[{"Style", "[", 
            RowBox[{
             RowBox[{"assoc", "[", "\"\<FullName\>\"", "]"}], ",", "Bold"}], 
            "]"}]}], ",", 
          RowBox[{"Text", "[", 
           RowBox[{"\"\<b. \>\"", "<>", 
            RowBox[{"DateString", "@", 
             RowBox[{"assoc", "[", "\"\<BirthDate\>\"", "]"}]}], "<>", 
            "\"\<, d. \>\"", "<>", 
            RowBox[{"DateString", "@", 
             RowBox[{"assoc", "[", "\"\<DeathDate\>\"", "]"}]}]}], "]"}], ",", 
          RowBox[{"Column", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Text", "[", 
              RowBox[{"Style", "[", 
               RowBox[{"#", ",", " ", "Medium"}], "]"}], "]"}], " ", "&"}], 
            " ", "/@", " ", 
            RowBox[{"assoc", "[", "\"\<NotableFacts\>\"", "]"}]}], "]"}]}], 
         "}"}], "]"}]}], "}"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Frame", "\[Rule]", "All"}], ",", 
    RowBox[{"ItemSize", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Scaled", "[", ".25", "]"}], ",", 
        RowBox[{"Scaled", "[", ".65", "]"}]}], "}"}], "}"}]}], ",", 
    RowBox[{"Alignment", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"Center", ",", "Left"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"Top", ",", "Top"}], "}"}]}], "}"}]}]}], "]"}]}]], "Input",
