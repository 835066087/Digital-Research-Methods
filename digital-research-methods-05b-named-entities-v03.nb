   RowBox[{"mccarthyismAssoc", ",", 
    RowBox[{"\"\<Hiss, Alger\>\"", "\[Rule]", 
     RowBox[{"getWAPersonPropertyAssoc", "[", "\"\<Alger Hiss\>\"", "]"}]}]}],
    "]"}], ";"}]], "Input",
