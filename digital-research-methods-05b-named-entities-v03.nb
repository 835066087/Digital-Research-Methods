Cell["Storing entity information in an association", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.651645449945039*^9, 3.6516454611598043`*^9}}],

Cell[TextData[{
 "Associations provide a convenient way to store a copy of any information \
that we retrieve about a particular entity. Since it takes a while to request \
external information, we don\[CloseCurlyQuote]t want to ask for the same \
thing more than once. The ",
 StyleBox["PersonData",
  FontWeight->"Bold"],
 " command allows us to specify an ",
 StyleBox["annotation",
  FontSlant->"Italic"],
 ", in this case ",
 StyleBox["PropertyAssociation",
  FontSlant->"Italic"],
 ", telling it how to return its results."
}], "Text",
 CellChangeTimes->{{3.651645464959532*^9, 3.651645469305105*^9}, {
  3.651651383073288*^9, 3.651651447149662*^9}, {3.651651555402566*^9, 
  3.651651597232379*^9}, {3.651651630808391*^9, 3.651651675813458*^9}}],

Cell[BoxData[
 RowBox[{"hooverAssoc", "=", 
  RowBox[{"PersonData", "[", 
   RowBox[{
    RowBox[{"Entity", "[", 
     RowBox[{"\"\<Person\>\"", ",", "\"\<JEdgarHoover::3737b\>\""}], "]"}], 
    ",", 
    RowBox[{"{", 
     RowBox[{
     "\"\<Entity\>\"", ",", "\"\<FullName\>\"", ",", "\"\<Image\>\"", ",", 
      "\"\<BirthDate\>\"", ",", "\"\<DeathDate\>\"", ",", 
      "\"\<NotableFacts\>\""}], "}"}], ",", "\"\<PropertyAssociation\>\""}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.6516515144921923`*^9, 3.651651542115054*^9}, {
  3.651651731164611*^9, 3.651651733163636*^9}, {3.651651778579026*^9, 
  3.651651797730755*^9}, {3.734973570465315*^9, 3.734973578540731*^9}},
 CellLabel->"In[50]:=",ExpressionUUID->"d7a69030-e00d-489b-b411-d4ebca9bdc13"],

Cell[BoxData[
 RowBox[{"Head", "[", "hooverAssoc", "]"}]], "Input",
