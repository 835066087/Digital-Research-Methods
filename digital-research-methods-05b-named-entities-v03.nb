Cell[BoxData[
 RowBox[{
  RowBox[{"getWAPersonPropertyAssoc", "[", "person_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"PersonData", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Interpreter", "[", "\"\<ComputedPerson\>\"", "]"}], "[", 
     "person", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{
     "\"\<Entity\>\"", ",", "\"\<FullName\>\"", ",", "\"\<Image\>\"", ",", 
      "\"\<BirthDate\>\"", ",", "\"\<DeathDate\>\"", ",", 
      "\"\<NotableFacts\>\""}], "}"}], ",", "\"\<PropertyAssociation\>\""}], 
   "]"}]}]], "Input",
