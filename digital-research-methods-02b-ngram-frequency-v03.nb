 RowBox[{
  RowBox[{"modalVerbs", "=", 
   RowBox[{"{", 
    RowBox[{
    "\"\<can\>\"", ",", "\"\<could\>\"", ",", "\"\<may\>\"", ",", 
     "\"\<might\>\"", ",", "\"\<must\>\"", ",", "\"\<shall\>\"", ",", 
     "\"\<should\>\"", ",", "\"\<will\>\"", ",", "\"\<would\>\""}], "}"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.647452147020494*^9, 3.64745218757296*^9}},
 CellLabel->
