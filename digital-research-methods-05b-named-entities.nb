Cell[BoxData[
 RowBox[{
  RowBox[{"getWAPersonPropertyAssoc", "[", "person_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"PersonData", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Interpreter", "[", "\"\<ComputedPerson\>\"", "]"}], "[", 
     "person", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{
     "\"\<Entity\>\"", ",", "\"\<FullName\>\"", ",", "\"\<Image\>\"", ",", 
      "\"\<BirthDate\>\"", ",", "\"\<DeathDate\>\"", ",", 
      "\"\<NotableFacts\>\""}], "}"}], ",", "\"\<PropertyAssociation\>\""}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.64182939296874*^9, 3.641829412373787*^9}, {
  3.6418294730478287`*^9, 3.641829503478922*^9}, {3.64182959503443*^9, 
  3.6418295994352217`*^9}, {3.641830457033346*^9, 3.641830466522045*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"formatWAPersonData", "[", "assoc_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Grid", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"assoc", "\[LeftDoubleBracket]", 
        RowBox[{"Key", "[", 
         RowBox[{"EntityProperty", "[", 
          RowBox[{"\"\<Person\>\"", ",", "\"\<Image\>\""}], "]"}], "]"}], 
        "\[RightDoubleBracket]"}], ",", 
       RowBox[{"Column", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Text", "@", 
           RowBox[{"Style", "[", 
            RowBox[{
             RowBox[{"assoc", "\[LeftDoubleBracket]", 
              RowBox[{"Key", "[", 
               RowBox[{"EntityProperty", "[", 
                RowBox[{"\"\<Person\>\"", ",", "\"\<FullName\>\""}], "]"}], 
               "]"}], "\[RightDoubleBracket]"}], ",", "Bold"}], "]"}]}], ",", 
          
          RowBox[{"Text", "[", 
           RowBox[{"\"\<b. \>\"", "<>", 
            RowBox[{"DateString", "@", 
             RowBox[{"assoc", "\[LeftDoubleBracket]", 
              RowBox[{"Key", "[", 
               RowBox[{"EntityProperty", "[", 
                RowBox[{"\"\<Person\>\"", ",", "\"\<BirthDate\>\""}], "]"}], 
               "]"}], "\[RightDoubleBracket]"}]}], "<>", "\"\<, d. \>\"", "<>", 
            RowBox[{"DateString", "@", 
             RowBox[{"assoc", "\[LeftDoubleBracket]", 
              RowBox[{"Key", "[", 
               RowBox[{"EntityProperty", "[", 
                RowBox[{"\"\<Person\>\"", ",", "\"\<DeathDate\>\""}], "]"}], 
               "]"}], "\[RightDoubleBracket]"}]}]}], "]"}], ",", 
          RowBox[{"Column", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Text", "[", 
              RowBox[{"Style", "[", 
               RowBox[{"#", ",", " ", "Medium"}], "]"}], "]"}], " ", "&"}], 
            " ", "/@", " ", 
            RowBox[{"assoc", "\[LeftDoubleBracket]", 
             RowBox[{"Key", "[", 
              RowBox[{"EntityProperty", "[", 
               RowBox[{"\"\<Person\>\"", ",", "\"\<NotableFacts\>\""}], "]"}],
               "]"}], "\[RightDoubleBracket]"}]}], "]"}]}], "}"}], "]"}]}], 
      "}"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Frame", "\[Rule]", "All"}], ",", 
    RowBox[{"ItemSize", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Scaled", "[", ".25", "]"}], ",", 
        RowBox[{"Scaled", "[", ".65", "]"}]}], "}"}], "}"}]}], ",", 
    RowBox[{"Alignment", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
