 RowBox[{"DictionaryLookup", "[", 
  RowBox[{"___", "~~", "\"\<govern\>\"", "~~", "___"}], "]"}]], "Input",
 CellChangeTimes->{3.647365517387375*^9},
 NumberMarks->False],

