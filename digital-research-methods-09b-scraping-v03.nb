 RowBox[{
  RowBox[{"biographyXMLstring", "=", 
   RowBox[{"StringRiffle", "[", 
    RowBox[{
     RowBox[{"Flatten", "[", 
      RowBox[{"Import", "[", 
       RowBox[{
       "\"\<http://classify.oclc.org/classify2/Classify?isbn=0143038273&\
summary=true\>\"", ",", "\"\<Data\>\""}], "]"}], "]"}], ",", "\"\<\\n\>\""}], 
    "]"}]}], ";"}]], "Input",
