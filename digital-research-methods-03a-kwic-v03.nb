 RowBox[{"Flatten", "[", 
  RowBox[{"StringCases", "[", 
   RowBox[{"kennanLongTelegramText", ",", 
    RowBox[{"Shortest", "[", 
     RowBox[{"\"\<\\\"\>\"", "~~", "__", "~~", "\"\<\\\"\>\""}], "]"}]}], 
   "]"}], "]"}]], "Input",
