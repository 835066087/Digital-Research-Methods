  RowBox[{"StringCases", "[", 
   RowBox[{"kennanLongTelegramText", ",", 
    RowBox[{"Shortest", "[", 
     RowBox[{
     "\"\<\[OpenCurlyDoubleQuote]\>\"", "~~", "__", "~~", " ", 
      "\"\<\[CloseCurlyDoubleQuote]\>\""}], "]"}]}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.734433007960054*^9, 3.734433010254973*^9}, {
