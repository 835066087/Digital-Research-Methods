Cell[TextData[{
 "Here are the bigrams in ",
 StyleBox["Pride and Prejudice",
  FontSlant->"Italic"],
 ", once stopwords have been removed."
}], "Text",
 CellChangeTimes->{{3.6826192116964417`*^9, 3.682619227431835*^9}}],

Cell[BoxData[
 RowBox[{"Short", "[", 
  RowBox[{"prideBigrams", ",", "4"}], "]"}]], "Input",
 CellChangeTimes->{{3.68261924054596*^9, 3.68261924956756*^9}}],

Cell[TextData[{
 "We can use ",
 StyleBox["Tally",
  FontWeight->"Bold"],
 " and ",
 StyleBox["Sort",
  FontWeight->"Bold"],
 " to create a list of bigram frequencies from a list of bigrams."
}], "Text",
 CellChangeTimes->{{3.6826191785934896`*^9, 3.682619202784767*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"prideBigramFrequencies", "=", 
   RowBox[{"Sort", "[", 
    RowBox[{
     RowBox[{"Tally", "[", "prideBigrams", "]"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
       ">", 
       RowBox[{"#2", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}],
       "&"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6826192558345623`*^9, 3.682619274313373*^9}}],

Cell["Here are the 50 most frequent bigrams.", "Text",
 CellChangeTimes->{{3.682619280022442*^9, 3.682619286525758*^9}}],

Cell[BoxData[
 RowBox[{"prideBigramFrequencies", "\[LeftDoubleBracket]", 
  RowBox[{"1", ";;", "50"}], "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.682619300872813*^9, 3.682619306963818*^9}}],

Cell["\<\
Who are the male characters? Who are the female ones? Which character(s) \
would be better captured with trigram frequencies? How do you know?\
\>", "Text",
 CellChangeTimes->{{3.6477054875917*^9, 3.64770549159164*^9}, {
   3.647705591259203*^9, 3.6477056194183893`*^9}, {3.647705659480888*^9, 
   3.647705685056211*^9}, {3.6477057522854967`*^9, 3.647705752917614*^9}, {
   3.6477076145548973`*^9, 3.647707645761269*^9}, 3.6826192920854177`*^9, {
   3.683284444670005*^9, 3.6832844475970078`*^9}}]
}, Open  ]]
