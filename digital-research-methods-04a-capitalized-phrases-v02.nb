Cell[CellGroupData[{

Cell["Text string", "Subsection",
 CellChangeTimes->{{3.682617804323388*^9, 3.682617805523432*^9}}],

Cell[TextData[{
 "The text of ",
 StyleBox["Pride and Prejudice",
  FontSlant->"Italic"],
 " is available as a ",
 StyleBox["string",
  FontSlant->"Italic"],
 " through the ",
 StyleBox["ExampleData",
  FontWeight->"Bold"],
 " command"
}], "Text",
 CellChangeTimes->{{3.682617612882946*^9, 3.68261764010561*^9}, {
  3.6826176762161417`*^9, 3.682617678160047*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"pride", "=", 
   RowBox[{"ExampleData", "[", 
    RowBox[{"{", 
     RowBox[{"\"\<Text\>\"", ",", "\"\<PrideAndPrejudice\>\""}], "}"}], 
    "]"}]}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{"Short", "[", 
  RowBox[{"pride", ",", "4"}], "]"}]], "Input",
 CellChangeTimes->{{3.682617651764888*^9, 3.6826176671204557`*^9}}],

Cell[TextData[{
 "As we did in previous classes, we will create a number of different ",
 StyleBox["representations",
  FontSlant->"Italic"],
 " of our text. These will allow us to answer different kinds of questions \
about the text."
}], "Text",
 CellChangeTimes->{{3.682617528101956*^9, 3.682617587947138*^9}, {
  3.683284362558055*^9, 3.683284365232424*^9}}]
}, Open  ]],

