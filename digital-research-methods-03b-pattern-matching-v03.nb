Cell[CellGroupData[{

Cell["String patterns", "Section",
 CellChangeTimes->{{3.6473429438950644`*^9, 3.647342944950231*^9}, {
   3.647353742456744*^9, 3.647353748014737*^9}, {3.681296708530692*^9, 
   3.6812967094017897`*^9}, 
   3.681658046473571*^9},ExpressionUUID->"f2ba4c78-5118-4859-a986-\
80ef6cf4dd48"],

Cell[TextData[{
 "We use the ",
 StyleBox["StringCases",
  FontWeight->"Bold"],
 " command to find parts of a string that match a string pattern."
}], "Text",
 CellChangeTimes->{{3.681658087597383*^9, 
  3.681658105268114*^9}},ExpressionUUID->"30fbb34d-4ee4-4cc9-bb84-\
1925521c5b8d"],

Cell[TextData[{
 "In string matching, a single underscore matches one character. Here are all \
the four-letter words in the Long Telegram that end in \
\[OpenCurlyDoubleQuote]ind\[CloseCurlyDoubleQuote]. The double tilde is \
shorthand for the ",
 StyleBox["StringExpression",
  FontWeight->"Bold"],
 " command. The ",
 StyleBox["Flatten",
  FontWeight->"Bold"],
 " command creates a flat list; the ",
 StyleBox["DeleteDuplicates",
  FontWeight->"Bold"],
 " command eliminates duplicates."
}], "Text",
 CellChangeTimes->{{3.647619132264821*^9, 3.647619210622655*^9}, {
  3.647619325353922*^9, 3.647619329754243*^9}, {3.6476193780959053`*^9, 
  3.647619405767445*^9}, {3.6816583042928*^9, 3.681658338283558*^9}, {
  3.734624549547228*^9, 
  3.734624556417349*^9}},ExpressionUUID->"308adfeb-2379-48f9-a994-\
b964405f15d3"],

Cell[BoxData[
 RowBox[{"DeleteDuplicates", "[", 
  RowBox[{"Flatten", "[", 
   RowBox[{"StringCases", "[", 
    RowBox[{"kennanWords", ",", 
     RowBox[{"_", "~~", "\"\<ind\>\""}]}], "]"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.681658247440962*^9, 3.681658300565868*^9}, {
  3.681658342915927*^9, 3.6816583631075897`*^9}, {3.7346245633618526`*^9, 
  3.734624567323131*^9}},
 CellLabel->"In[59]:=",ExpressionUUID->"e86da04b-b6a5-4fdd-b434-9a8ee6942133"],

Cell[TextData[{
 "Note that one of these, ",
 StyleBox["-ind",
  FontSlant->"Italic"],
 ", is a piece of a word. We will soon learn how to limit our matching to \
whole words only."
}], "Text",
 CellChangeTimes->{{3.734624623807167*^9, 3.73462465046974*^9}, {
  3.734624735242589*^9, 
  3.73462481755159*^9}},ExpressionUUID->"9a21047b-5fb2-47b7-a932-\
e7175b8b54f1"],

Cell["\<\
A double underscore matches one or more characters.  All the words (and some \
pieces of words) that end in \[OpenCurlyDoubleQuote]ing\
\[CloseCurlyDoubleQuote]:\
\>", "Text",
 CellChangeTimes->{{3.6476193408250437`*^9, 3.647619375927836*^9}, {
  3.647619413214641*^9, 3.647619413582389*^9}, {3.734624900949246*^9, 
  3.73462490594037*^9}},ExpressionUUID->"481c2371-0a0d-4c90-a4f1-\
6c31f9241a85"],

Cell[BoxData[
 RowBox[{"DeleteDuplicates", "[", 
  RowBox[{"Flatten", "[", 
   RowBox[{"StringCases", "[", 
    RowBox[{"kennanWords", ",", 
     RowBox[{"__", "~~", "\"\<ing\>\""}]}], "]"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.6476192147209*^9, 3.647619229357959*^9}, {
   3.647619303203966*^9, 3.64761932076292*^9}, {3.647619421087462*^9, 
   3.647619423990385*^9}, 3.681658395868519*^9, {3.7346245884329987`*^9, 
   3.734624589016231*^9}},
 CellLabel->"In[60]:=",ExpressionUUID->"f284bca3-6730-4d35-be59-00571a930c33"],

Cell["\<\
All the words that begin with \[OpenCurlyDoubleQuote]str\
\[CloseCurlyDoubleQuote]. Again, a number of these are pieces of words\
\>", "Text",
 CellChangeTimes->{{3.681658418145041*^9, 3.6816584255360947`*^9}, {
  3.73462482231925*^9, 
  3.7346248394306383`*^9}},ExpressionUUID->"1d99ebcc-0072-45e6-9720-\
c3c3ede17e40"],

Cell[BoxData[
 RowBox[{"DeleteDuplicates", "[", 
  RowBox[{"Flatten", "[", 
   RowBox[{"StringCases", "[", 
    RowBox[{"kennanWords", ",", 
     RowBox[{"\"\<str\>\"", "~~", "__"}]}], "]"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.6476192147209*^9, 3.647619229357959*^9}, {
   3.647619303203966*^9, 3.64761932076292*^9}, {3.647619421087462*^9, 
   3.647619423990385*^9}, 3.681658395868519*^9, {3.681658434170199*^9, 
   3.681658440103932*^9}, {3.734624595760716*^9, 3.7346245963038588`*^9}},
 CellLabel->"In[61]:=",ExpressionUUID->"4a747778-7c1a-47de-a4bf-b437115cc8d9"]
}, Open  ]]
