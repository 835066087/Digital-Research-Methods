Cell[TextData[{
 "This function uses ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "\[CloseCurlyQuote]s graph features to construct a list of links from Walter \
Lippmann to associated names, and from those names to each of their neighbors \
in turn"
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.657299823004222*^9, 3.6572998807587957`*^9}, {3.7368747288333473`*^9, 
  3.736874731121056*^9}},ExpressionUUID->"483a0beb-046d-4795-9e12-\
ab3766ccc951"],

Cell[BoxData[
 RowBox[{
  RowBox[{"getWorldCatIdentitiesNeighborhood", "[", 
   RowBox[{"id_", ",", "namestr_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"version", " ", "1", " ", "June", " ", "2018"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"neighbor1", ",", "outgraph", ",", "nname", ",", "ntemp"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"neighbor1", "=", 
      RowBox[{"getWorldCatIdentitiesANamesIDs", "[", "id", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"outgraph", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"namestr", "\[Rule]", 
          RowBox[{
          "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
         "&"}], ",", "neighbor1"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"nname", "=", 
         RowBox[{
         "n", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"ntemp", "=", 
         RowBox[{"getWorldCatIdentitiesANamesIDs", "[", 
          RowBox[{"n", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
           "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"outgraph", "=", 
         RowBox[{"Join", "[", 
          RowBox[{"outgraph", ",", 
           RowBox[{"Map", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"nname", "\[Rule]", 
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
              "&"}], ",", "ntemp"}], "]"}]}], "]"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"n", ",", "neighbor1"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "outgraph", "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.513613235731237*^9, 3.513613459965186*^9}, {
   3.513613527939415*^9, 3.5136136774501867`*^9}, 3.643823377502389*^9, {
   3.643824730109852*^9, 3.643824730764441*^9}, {3.64382543234251*^9, 
   3.643825434803637*^9}, {3.6438254708211813`*^9, 3.643825478755319*^9}, {
