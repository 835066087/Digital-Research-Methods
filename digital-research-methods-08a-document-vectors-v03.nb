  3.653121391260528*^9}},ExpressionUUID->"cceca486-2f4b-4b7d-8373-\
bd277f9a2cd3"],

Cell[BoxData[
 RowBox[{"Dot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"0", ",", "0"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"0", ",", "1"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.649250347005537*^9, 3.649250364294126*^9}},
 CellLabel->"In[35]:=",ExpressionUUID->"a74f2e75-7b9c-4f42-9532-7c064468950a"],

