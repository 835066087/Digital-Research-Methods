Cell[BoxData[
 RowBox[{"firstOnTheMoonFrequentTerms", "=", 
  RowBox[{
   RowBox[{"Select", "[", 
    RowBox[{
     RowBox[{"Sort", "[", 
      RowBox[{
       RowBox[{"Tally", "[", 
        RowBox[{"DeleteStopwords", "[", 
         RowBox[{"Flatten", "[", 
          RowBox[{"ToLowerCase", "[", 
           RowBox[{"TextWords", "[", "firstOnTheMoonTrimmed", "]"}], "]"}], 
          "]"}], "]"}], "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}],
          ">", 
         RowBox[{
         "#2", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
        "&"}]}], "]"}], ",", 
     RowBox[{
