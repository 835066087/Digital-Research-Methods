Cell["\<\
Here are non-stopwords that occur frequently in each of the two speeches\
\>", "Text",
 CellChangeTimes->{{3.681302704712099*^9, 3.681302746262865*^9}}],

Cell[BoxData["hstJan1946FrequentTerms"], "Input",
 CellChangeTimes->{{3.681301897381116*^9, 3.6813019240097437`*^9}, {
  3.734540830061969*^9, 3.734540830229211*^9}, {3.734540867148714*^9, 
  3.734540869037956*^9}},
 CellLabel->"In[33]:=",ExpressionUUID->"2f7eff54-c87e-41da-bdca-22870eef3493"],

Cell[BoxData[
 RowBox[{
  RowBox[{"hstJan1950FrequencyAssociation", "=", 
   RowBox[{"WordCounts", "[", 
    RowBox[{"hstJan1950String", ",", 
     RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.681300458899687*^9, 3.6813005073589*^9}, {
  3.681300608496553*^9, 3.681300638767531*^9}, {3.6813028935065947`*^9, 
  3.6813028988419247`*^9}, {3.734540877581012*^9, 3.734540891326849*^9}},
 CellLabel->"In[34]:=",ExpressionUUID->"71969f79-d92d-42ec-b1d4-b850c0d58879"],

Cell[BoxData[
 RowBox[{"hstJan1950FrequentTerms", "=", 
  RowBox[{"DeleteStopwords", "[", 
   RowBox[{"Keys", "[", 
    RowBox[{"hstJan1950FrequencyAssociation", "\[LeftDoubleBracket]", 
     RowBox[{"1", ";;", "100"}], "\[RightDoubleBracket]"}], "]"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.681301897381116*^9, 3.6813019240097437`*^9}, {
  3.6813027705832443`*^9, 3.681302776222514*^9}, {3.734540898836232*^9, 
  3.7345409090425043`*^9}},
 CellLabel->"In[35]:=",ExpressionUUID->"ab342016-15da-43e9-b60e-8d845a880d06"],

Cell[TextData[{
 "We can use the ",
 StyleBox["Complement",
  FontWeight->"Bold"],
 " command to find all of the words which appear in one list, but not \
another. Here are the common words in the 1946 speech that do not appear \
frequently in the 1950 speech."
}], "Text",
 CellChangeTimes->{{3.681302911128481*^9, 3.681302992429637*^9}, {
  3.7345409277946243`*^9, 
  3.734540958514908*^9}},ExpressionUUID->"0b58776f-6241-4293-a878-\
86e490cced2a"],

Cell[BoxData[
 RowBox[{"Complement", "[", 
  RowBox[{"hstJan1946FrequentTerms", ",", "hstJan1950FrequentTerms"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.681302940249796*^9, 3.681302955728146*^9}, {
  3.7345409438905907`*^9, 3.734540951299686*^9}},
 CellLabel->"In[36]:=",ExpressionUUID->"47da26af-f008-45cd-ab9d-d84ec5ced05f"],

Cell[TextData[{
 "Here are the common words in the 1950 speech that do not appear frequently \
in the 1946 speech. Note that the order of the arguments to ",
 StyleBox["Complement",
  FontWeight->"Bold"],
 " matters."
}], "Text",
 CellChangeTimes->{{3.681302998238523*^9, 3.681303010069029*^9}, {
  3.734540979081382*^9, 3.734540992615295*^9}, {3.7345410639813232`*^9, 
  3.7345410771321917`*^9}},ExpressionUUID->"25dce7af-65dc-4bad-b623-\
d033a62d496e"],

