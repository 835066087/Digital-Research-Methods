Cell[CellGroupData[{

Cell["Charles Lyell (1797-1875)", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6572894133521137`*^9, 3.657289414966826*^9}, {3.6572895630784893`*^9, 
  3.657289569148965*^9}}],

Cell["\<\
For our example, we are going to retrieve information from the Open Library \
API about the geologist Charles Lyell\
\>", "Text",
 CellChangeTimes->{{3.657289529314231*^9, 3.657289559822118*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"PersonData", "[", 
   RowBox[{
    TemplateBox[{"\"Charles Lyell\"",RowBox[{"Entity", "[", 
        RowBox[{"\"Person\"", ",", "\"CharlesLyell::68zmm\""}], "]"}],
      "\"Entity[\\\"Person\\\", \\\"CharlesLyell::68zmm\\\"]\"","\"person\""},
     
     "Entity"], ",", 
    RowBox[{"{", 
     RowBox[{
     "\"\<Image\>\"", ",", "\"\<BirthDate\>\"", ",", "\"\<DeathDate\>\""}], 
     "}"}]}], "]"}], "//", "TableForm"}]], "Input",
 CellChangeTimes->{{3.6572894585072317`*^9, 3.657289524611586*^9}},
 NumberMarks->False],

Cell[BoxData[
 RowBox[{
  RowBox[{"TextSentences", "[", 
   RowBox[{"WikipediaData", "[", "\"\<Charles Lyell\>\"", "]"}], "]"}], 
  "\[LeftDoubleBracket]", 
  RowBox[{"1", ";;", "8"}], "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.657289339272758*^9, 3.6572893823947563`*^9}}]
}, Open  ]]
}, Open  ]],

