Cell[CellGroupData[{

Cell["Connecting to a web service (API)", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.65728958717223*^9, 3.6572895946740513`*^9}}],

Cell[TextData[{
 "We use the ",
 StyleBox["ServiceConnect",
  FontWeight->"Bold"],
 " command to connect to an API. ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " returns a ",
 StyleBox["ServiceObject",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.6572895972257433`*^9, 3.657289639120656*^9}}],

Cell[BoxData[
 RowBox[{"openLibrary", "=", 
  RowBox[{"ServiceConnect", "[", "\"\<OpenLibrary\>\"", "]"}]}]], "Input",
 CellChangeTimes->{{3.646759997942033*^9, 3.646760007676362*^9}}]
}, Open  ]]
}, Open  ]],

