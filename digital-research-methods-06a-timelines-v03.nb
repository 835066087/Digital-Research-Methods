Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"startdate", "=", 
      RowBox[{"{", 
       RowBox[{"1947", ",", "5", ",", "1"}], "}"}]}], ",", 
     RowBox[{"enddate", "=", 
      RowBox[{"{", 
       RowBox[{"1947", ",", "8", ",", "31"}], "}"}]}]}], "}"}], ",", 
   RowBox[{"TableForm", "[", 
    RowBox[{"Sort", "[", 
     RowBox[{
      RowBox[{"Values", "[", 
       RowBox[{"Select", "[", 
        RowBox[{"histevents", ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"Order", "[", 
              RowBox[{
               RowBox[{"DateList", "[", "startdate", "]"}], ",", 
               RowBox[{"DateList", "[", 
                RowBox[{
                "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
                "]"}]}], "]"}], "\[Equal]", "1"}], "&&", 
            RowBox[{
             RowBox[{"Order", "[", 
              RowBox[{
               RowBox[{"DateList", "[", "enddate", "]"}], ",", 
               RowBox[{"DateList", "[", 
                RowBox[{
                "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
                "]"}]}], "]"}], "\[Equal]", 
             RowBox[{"-", "1"}]}]}], ")"}], "&"}]}], "]"}], "]"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
        "<", 
        RowBox[{
        "#2", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
