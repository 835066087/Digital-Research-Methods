  3.6418295994352217`*^9}, {3.641830457033346*^9, 3.641830466522045*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"formatWAPersonData", "[", "assoc_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"version", " ", "20160920"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"Grid", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"assoc", "[", "\"\<Image\>\"", "]"}], ",", 
       RowBox[{"Column", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Text", "@", 
           RowBox[{"Style", "[", 
            RowBox[{
             RowBox[{"assoc", "[", "\"\<FullName\>\"", "]"}], ",", "Bold"}], 
            "]"}]}], ",", 
          RowBox[{"Text", "[", 
           RowBox[{"\"\<b. \>\"", "<>", 
            RowBox[{"DateString", "@", 
             RowBox[{"assoc", "[", "\"\<BirthDate\>\"", "]"}]}], "<>", 
            "\"\<, d. \>\"", "<>", 
            RowBox[{"DateString", "@", 
             RowBox[{"assoc", "[", "\"\<DeathDate\>\"", "]"}]}]}], "]"}], ",", 
          RowBox[{"Column", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Text", "[", 
              RowBox[{"Style", "[", 
               RowBox[{"#", ",", " ", "Medium"}], "]"}], "]"}], " ", "&"}], 
            " ", "/@", " ", 
            RowBox[{"assoc", "[", "\"\<NotableFacts\>\"", "]"}]}], "]"}]}], 
         "}"}], "]"}]}], "}"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Frame", "\[Rule]", "All"}], ",", 
    RowBox[{"ItemSize", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Scaled", "[", ".25", "]"}], ",", 
        RowBox[{"Scaled", "[", ".65", "]"}]}], "}"}], "}"}]}], ",", 
    RowBox[{"Alignment", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"Center", ",", "Left"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"Top", ",", "Top"}], "}"}]}], "}"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.641491826288348*^9, 3.641491880204611*^9}, {
  3.649058510228985*^9, 3.649058552576703*^9}, {3.6833009159797688`*^9, 
  3.6833010257744913`*^9}, {3.683301061375318*^9, 3.683301073412898*^9}}],

