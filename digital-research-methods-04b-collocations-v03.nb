Cell[BoxData[
 RowBox[{
  RowBox[{"getOccurrences", "[", 
   RowBox[{"txtblks_", ",", "ngram_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Map", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Length", "[", 
      RowBox[{"SequenceCases", "[", 
       RowBox[{"#", ",", "ngram"}], "]"}], "]"}], "&"}], ",", "txtblks"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.647748516710093*^9, 3.647748582265139*^9}},
 CellLabel->"In[41]:=",ExpressionUUID->"55f7dbe9-33d9-4908-92df-2c3197662aa8"],

Cell[TextData[{
 "The ",
 StyleBox["getOccurrences",
  FontSlant->"Italic"],
 " function returns the number of times an n-gram appears in a particular \
block."
}], "Text",
 CellChangeTimes->{{3.6832972702702*^9, 3.683297300532498*^9}}],

