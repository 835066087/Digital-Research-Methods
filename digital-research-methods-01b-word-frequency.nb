Cell[TextData[{
 "The ",
 StyleBox["Select",
  FontWeight->"Bold"],
 " command allows us to pull out elements that meet particular criteria. We \
describe what we are looking for with a special function (again, just copy \
the code for now if you need it)."
}], "Text",
 CellChangeTimes->{{3.647358493976321*^9, 3.647358512727977*^9}, {
  3.647358583173877*^9, 3.647358597668651*^9}, {3.647358628908535*^9, 
  3.6473587242880507`*^9}}],

Cell["\<\
Here are all of the words that appear more than once in the Gettysburg \
Address.\
\>", "Text",
 CellChangeTimes->{{3.6473595773931723`*^9, 3.6473595902482557`*^9}}],

Cell[BoxData[
 RowBox[{"Sort", "[", 
  RowBox[{
   RowBox[{"Select", "[", 
    RowBox[{"gettysburgFrequencies", ",", 
     RowBox[{
      RowBox[{
       RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
       ">", "1"}], "&"}]}], "]"}], ",", 
   RowBox[{
    RowBox[{
     RowBox[{"#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
     ">", 
     RowBox[{"#2", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
    "&"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.64735851457014*^9, 3.6473585769516087`*^9}}],

Cell[TextData[{
 "We can use the ",
