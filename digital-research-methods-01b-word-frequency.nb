Cell["Comparing texts", "Section",
 CellChangeTimes->{{3.647358433843246*^9, 3.647358435786759*^9}}],

Cell["\<\
Here are all the words that occur more than once in JFK\[CloseCurlyQuote]s \
inaugural address.\
\>", "Text",
 CellChangeTimes->{{3.6473603392212143`*^9, 3.647360358556126*^9}}],

Cell[BoxData[
 RowBox[{"jfkCommon", "=", 
  RowBox[{
   RowBox[{"Sort", "[", 
    RowBox[{
     RowBox[{"Select", "[", 
      RowBox[{
       RowBox[{"Tally", "[", 
        RowBox[{"ToLowerCase", "[", 
         RowBox[{"TextWords", "[", "jfk", "]"}], "]"}], "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
