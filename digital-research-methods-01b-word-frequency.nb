Cell[TextData[{
 "The ",
 StyleBox["Part",
  FontWeight->"Bold"],
 " command gives you a part of a list. Elements are numbered beginning with 1."
}], "Text",
 CellChangeTimes->{{3.647359729819696*^9, 3.647359751714473*^9}}],

Cell[BoxData[
 RowBox[{"Part", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "\"\<a\>\"", ",", "\"\<b\>\"", ",", "\"\<c\>\"", ",", "\"\<d\>\""}], 
    "}"}], ",", "2"}], "]"}]], "Input",
 CellChangeTimes->{{3.647359755987976*^9, 3.647359773793683*^9}}],

Cell["Usually you write this using double square brackets.", "Text",
 CellChangeTimes->{{3.647359781513031*^9, 3.6473597946967907`*^9}}],

