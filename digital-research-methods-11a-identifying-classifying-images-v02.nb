Cell[CellGroupData[{

Cell["Image classification, Part 1: Functions", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6566021724177437`*^9, 3.656602183679378*^9}}],

Cell[TextData[{
 "Use ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "\[CloseCurlyQuote]s powerful new machine learning features to automatically \
classify extracted images into photographs and drawings (further subdivision \
is easy on large datasets)."
}], "Text",
 CellChangeTimes->{3.643652601262424*^9}],

Cell["Here are some functions we will need", "Text",
 CellChangeTimes->{{3.6566022109749403`*^9, 3.6566022196618023`*^9}, 
   3.68736895346227*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"standarizePage", "[", "pg_", "]"}], ":=", 
  RowBox[{"ColorNegate", "[", 
   RowBox[{"MorphologicalBinarize", "[", 
    RowBox[{
     RowBox[{"ColorConvert", "[", 
      RowBox[{"pg", ",", "\"\<Grayscale\>\""}], "]"}], ",", 
     RowBox[{"Method", "\[Rule]", "\"\<MinimumError\>\""}]}], "]"}], 
   "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"identifyImageBoundingBoxes", "[", "spg_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"ComponentMeasurements", "[", 
   RowBox[{
    RowBox[{"DeleteSmallComponents", "[", "spg", "]"}], ",", 
    "\[IndentingNewLine]", "\"\<BoundingBox\>\""}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"getExtractedImageList", "[", "pg_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"bblist", "=", 
      RowBox[{"identifyImageBoundingBoxes", "[", 
       RowBox[{"standarizePage", "[", "pg", "]"}], "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Return", "[", 
     RowBox[{"Map", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"ImageTrim", "[", 
         RowBox[{"pg", ",", "#"}], "]"}], "&"}], ",", 
       RowBox[{"bblist", "\[LeftDoubleBracket]", 
        RowBox[{"All", ",", "2"}], "\[RightDoubleBracket]"}]}], "]"}], 
     "]"}]}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"highlightExtractedImages", "[", "pg_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"bblist", "=", 
      RowBox[{"identifyImageBoundingBoxes", "[", 
       RowBox[{"standarizePage", "[", "pg", "]"}], "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Show", "[", 
     RowBox[{"pg", ",", 
      RowBox[{"Graphics", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"EdgeForm", "[", 
          RowBox[{"{", "Orange", "}"}], "]"}], ",", 
         RowBox[{"Opacity", "[", "0", "]"}], ",", 
         RowBox[{"Rectangle", "@@@", " ", 
          RowBox[{"bblist", "\[LeftDoubleBracket]", 
           RowBox[{"All", ",", "2"}], "\[RightDoubleBracket]"}]}]}], "}"}], 
       "]"}], ",", 
      RowBox[{"ImageSize", "\[Rule]", "Small"}]}], "]"}]}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"demoImageExtraction", "[", "pgimglist_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"returnlist", "=", 
      RowBox[{"{", "}"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"returnlist", "=", 
        RowBox[{"Append", "[", 
         RowBox[{"returnlist", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"highlightExtractedImages", "[", "i", "]"}], ",", 
            RowBox[{"Last", "[", 
             RowBox[{"getExtractedImageList", "[", "i", "]"}], "]"}]}], 
           "}"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"i", ",", "pgimglist"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "returnlist", "]"}]}]}], "]"}]}]], "Input"]
}, Open  ]]
}, Open  ]],

