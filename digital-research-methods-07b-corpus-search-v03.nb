 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6529711301463747`*^9, 
  3.652971133024641*^9}},ExpressionUUID->"88dec51e-06ba-4a41-8c57-\
d49216bba9b1"],

Cell[TextData[{
 "Now we are going to create indexes for each of the novels, and then ",
 StyleBox["Merge",
  FontWeight->"Bold"],
 " them into one large one. Once again we test to see if the merged index \
already exists before creating it. Once we create it, we ",
 StyleBox["Put",
  FontWeight->"Bold"],
 " it in a file, so we can ",
 StyleBox["Get",
  FontWeight->"Bold"],
 " it later rather than rebuild it."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
   3.652973284495618*^9, 3.6529733422607403`*^9}, 
   3.6529734054844418`*^9},ExpressionUUID->"c659b22c-09d4-483d-9eab-\
fabd33ac5c47"],

Cell[BoxData[
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"FileExistsQ", "[", "\"\<huacIndex.assoc.m\>\"", "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"huacIndex", "=", 
     RowBox[{"Get", "[", "\"\<huacIndex.assoc.m\>\"", "]"}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"huacIndex", "=", 
      RowBox[{"Merge", "[", 
       RowBox[{
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"indexWords", "[", 
            RowBox[{"huacTexts", ",", 
             RowBox[{"FileNameJoin", "[", 
              RowBox[{"{", 
               RowBox[{"#", "<>", "\"\<.txt\>\""}], "}"}], "]"}]}], "]"}], 
           "&"}], ",", "huacIDs"}], "]"}], ",", "Identity"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Put", "[", 
      RowBox[{"huacIndex", ",", "\"\<freemanIndex.assoc.m\>\""}], "]"}]}]}], 
   "]"}], ";"}]], "Input",
 CellChangeTimes->{{3.652973373269121*^9, 3.652973426219881*^9}, {
  3.652973531169406*^9, 3.652973550344983*^9}, {3.735145056571941*^9, 
  3.7351450828662567`*^9}},
 CellLabel->"In[51]:=",ExpressionUUID->"d315c8f3-22bb-4774-aca5-fae77f36183c"],

