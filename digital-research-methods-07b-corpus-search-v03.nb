Cell[BoxData[
 RowBox[{
  RowBox[{"searchTermContext", "[", 
   RowBox[{"textassoc_", ",", "wordpos_", ",", "filename_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"textassoc", "[", "filename", "]"}], "\[LeftDoubleBracket]", 
   RowBox[{
    RowBox[{"wordpos", "-", "3"}], ";;", 
    RowBox[{"wordpos", "+", "3"}]}], "\[RightDoubleBracket]"}]}]], "Input",
 CellChangeTimes->{{3.6457191110191298`*^9, 3.645719206322517*^9}, {
  3.645719276909635*^9, 3.6457192838370953`*^9}, {3.645719842935236*^9, 
  3.645719846332252*^9}},
 CellLabel->"In[56]:=",ExpressionUUID->"d4e42514-9a14-4c8b-b0bb-a552ebdacd03"],

Cell[BoxData[
 RowBox[{
  RowBox[{"displaySearch", "[", 
   RowBox[{"textassoc_", ",", "indexassoc_", ",", "term_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "results", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"results", "=", "\[IndentingNewLine]", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Thread", "[", 
           RowBox[{"List", "[", 
            RowBox[{
             RowBox[{"Values", "@", 
              RowBox[{
               RowBox[{"indexassoc", "[", "term", "]"}], 
               "\[LeftDoubleBracket]", "#", "\[RightDoubleBracket]"}]}], ",", 
             
             RowBox[{"Keys", "@", 
              RowBox[{
               RowBox[{"indexassoc", "[", "term", "]"}], 
               "\[LeftDoubleBracket]", "#", "\[RightDoubleBracket]"}]}]}], 
            "]"}], "]"}], "&"}], "/@", 
         RowBox[{"Range", "[", 
          RowBox[{"Length", "[", 
           RowBox[{"Keys", "@", 
            RowBox[{"indexassoc", "[", "term", "]"}]}], "]"}], "]"}]}], ",", 
        "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Framed", "[", 
      RowBox[{"Pane", "[", 
       RowBox[{
        RowBox[{"Style", "[", 
         RowBox[{
          RowBox[{"TableForm", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Append", "[", 
              RowBox[{
               RowBox[{"searchTermContext", "[", 
                RowBox[{"textassoc", ",", 
                 RowBox[{
                 "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
                 ",", 
                 RowBox[{
                 "#", "\[LeftDoubleBracket]", "2", 
                  "\[RightDoubleBracket]"}]}], "]"}], ",", 
               RowBox[{"Style", "[", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{
                   "#", "\[LeftDoubleBracket]", "2", 
                    "\[RightDoubleBracket]"}], ",", 
                   RowBox[{
                   "#", "\[LeftDoubleBracket]", "1", 
                    "\[RightDoubleBracket]"}]}], "}"}], ",", 
                 RowBox[{"Darker", "@", "Green"}]}], "]"}]}], "]"}], "&"}], "/@",
             "results"}], "]"}], ",", "Small"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"Full", ",", "200"}], "}"}], ",", 
        RowBox[{"Scrollbars", "\[Rule]", "True"}]}], "]"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.645720354655745*^9, 3.645720593118178*^9}, {
  3.645720632461602*^9, 3.645720747368436*^9}, {3.64572082191881*^9, 
