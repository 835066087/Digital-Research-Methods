Cell["\<\
Given a base URL, an identifier, the path to a download directory, and a file \
extension, this function downloads the file from the Internet Archive\
\>", "Text",
 CellChangeTimes->{{3.655230110221574*^9, 3.655230154443007*^9}, {
  3.655233643659685*^9, 3.655233653618187*^9}, {3.7354881223471518`*^9, 
  3.73548812680208*^9}},ExpressionUUID->"b20fcd9e-e3ce-4472-8628-\
d02c7d5ab022"],

Cell[BoxData[
 RowBox[{
  RowBox[{"downloadIAFile", "[", 
   RowBox[{"idstr_", ",", "dirstr_", ",", "ext_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"baseurl", ",", "target", ",", "fname"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Pause", "[", "2", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"baseurl", "=", "\"\<https://archive.org/download/\>\""}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"target", "=", 
      RowBox[{"FileNameJoin", "[", 
       RowBox[{"{", 
        RowBox[{"dirstr", ",", 
         RowBox[{"idstr", "<>", "ext"}]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"FileExistsQ", "[", "target", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"PrintTemporary", "[", 
        RowBox[{
        "\"\<File \>\"", "<>", "target", "<>", 
         "\"\< was already downloaded\>\""}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"PrintTemporary", "[", 
         RowBox[{"\"\<Downloading \>\"", "<>", "target"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"fname", "=", "\[IndentingNewLine]", 
         RowBox[{"Select", "[", 
          RowBox[{
           RowBox[{"StringCases", "[", 
            RowBox[{
             RowBox[{"URLFetch", "[", 
              RowBox[{
               RowBox[{"baseurl", "<>", "idstr"}], ",", "\"\<Content\>\""}], 
              "]"}], ",", 
             RowBox[{
              RowBox[{"Shortest", "[", 
               RowBox[{
               "\"\<href=\\\"\>\"", "~~", "x__", "~~", "\"\<\\\"\>\""}], 
               "]"}], "\[Rule]", "x"}]}], "]"}], ",", 
           RowBox[{
            RowBox[{"StringContainsQ", "[", 
             RowBox[{"#", ",", "ext"}], "]"}], "&"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"fname", "\[NotEqual]", 
           RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"URLSave", "[", 
           RowBox[{
            RowBox[{"baseurl", "<>", "idstr", "<>", "\"\</\>\"", "<>", 
             RowBox[{"First", "@", "fname"}]}], ",", "target"}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"PrintTemporary", "[", 
           RowBox[{"\"\<No text file for \>\"", "<>", "target"}], "]"}]}], 
         "]"}]}]}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.643304040419911*^9, 3.6433041061418533`*^9}, {
   3.64330483976549*^9, 3.6433050345844717`*^9}, {3.643305069023876*^9, 
