  RowBox[{
  "\"\<https://web.archive.org/web/20180516195401/http://digitalarchive.\
wilsoncenter.org/document/116188.pdf?v=3379f0c03457ecbe104be8895b9a8ea8\>\"", 
   ",", "\"\<PageCount\>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.646763032460813*^9, 3.646763117222975*^9}, {
   3.6471047308861933`*^9, 3.647104732908825*^9}, 3.735489325492875*^9, {
   3.7354894331969147`*^9, 3.735489484705213*^9}, 3.735489639441642*^9},
