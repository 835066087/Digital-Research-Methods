Cell[CellGroupData[{

Cell["Retrieving full text", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6572926083076363`*^9, 3.657292612682927*^9}}],

Cell[TextData[{
 "If full text is available online, we can get that, too. Here we get the \
full text of Lyell\[CloseCurlyQuote]s ",
 StyleBox["Manual of Elementary Geology ",
  FontSlant->"Italic"],
 "using the Open Library ID"
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.657292623674837*^9, 3.657292662174262*^9}, {3.65729269951478*^9, 
  3.657292743678385*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"lyellManual", "=", 
   RowBox[{"openLibrary", "[", 
    RowBox[{"\"\<BookText\>\"", ",", 
     RowBox[{"{", 
      RowBox[{"\"\<BibKeys\>\"", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{"\"\<OLID\>\"", ",", "\"\<OL25467158M\>\""}], "}"}]}], 
      "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.646760332057314*^9, 3.646760336068592*^9}, {
  3.646760885054788*^9, 3.646760949540515*^9}}],

Cell["Now we can analyze the text like any other.", "Text",
 CellChangeTimes->{{3.64676096155687*^9, 3.646760970235602*^9}}],

Cell[BoxData[
 RowBox[{"TextWords", "[", 
  RowBox[{"StringTake", "[", 
   RowBox[{
    RowBox[{"lyellManual", "[", "1", "]"}], ",", "2500"}], "]"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.646760975231889*^9, 3.646761018011099*^9}, {
  3.6572927809639482`*^9, 3.657292802832693*^9}}],

Cell[BoxData[
 RowBox[{"WordCloud", "[", 
  RowBox[{"DeleteStopwords", "[", 
   RowBox[{"ToLowerCase", "[", 
    RowBox[{"TextWords", "[", 
