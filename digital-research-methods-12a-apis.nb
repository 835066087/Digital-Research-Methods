Cell[CellGroupData[{

Cell["Searching for works by Charles Lyell", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6572896522674723`*^9, 3.657289658234737*^9}}],

Cell[TextData[{
 "Next we ask for up to 20 works authored by Lyell. Note that ",
 StyleBox["openLibrary",
  FontSlant->"Italic"],
 " in this context refers to our ",
 StyleBox["ServiceObject",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.657289660994473*^9, 3.657289717587823*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "=", 
  RowBox[{"openLibrary", "[", 
   RowBox[{"\"\<BookSearch\>\"", ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\"\<Author\>\"", "\[Rule]", "\"\<Charles Lyell\>\""}], ",", 
      RowBox[{"\"\<MaxItems\>\"", "\[Rule]", "20"}]}], "}"}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.646760032191082*^9, 3.646760087278082*^9}}]
}, Open  ]]
}, Open  ]],

