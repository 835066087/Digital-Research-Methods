Cell["Datasets", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.657289724323123*^9, 3.657289725954753*^9}}],

Cell[TextData[{
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " returns results in the form of a dataset. This is a hierarchical data \
structure that can contain numbers, strings, lists, associations and other \
elements."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.657289792323992*^9, 3.657289802817878*^9}, {3.657292127189501*^9, 
  3.657292161447806*^9}}],

Cell["Take a row from a dataset", "Text",
 CellChangeTimes->{{3.657292179262311*^9, 3.657292182325199*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "[", "2", "]"}]], "Input",
 CellChangeTimes->{{3.646760163668606*^9, 3.646760166946803*^9}, 
   3.6467608366972713`*^9, {3.657292408527883*^9, 3.657292409950636*^9}}],

Cell["Take a row and column", "Text",
 CellChangeTimes->{{3.6572921981240788`*^9, 3.6572922018431253`*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "[", 
  RowBox[{"2", ",", "3"}], "]"}]], "Input",
 CellChangeTimes->{{3.646760163668606*^9, 3.646760166946803*^9}, 
   3.6467608366972713`*^9, {3.6572922098750553`*^9, 3.657292211034068*^9}, {
   3.65729241363055*^9, 3.657292415494177*^9}}],

Cell[TextData[{
 "Since this column contains a list, we can use ",
 StyleBox["Part",
  FontWeight->"Bold"],
 " to request elements from it"
}], "Text",
 CellChangeTimes->{{3.657292236119069*^9, 3.657292247869897*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "[", 
  RowBox[{"2", ",", "6"}], "]"}]], "Input",
 CellChangeTimes->{{3.646760163668606*^9, 3.646760166946803*^9}, 
   3.6467608366972713`*^9, {3.6572922098750553`*^9, 3.657292228775881*^9}, {
   3.657292417846118*^9, 3.657292420373747*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "[", 
  RowBox[{"2", ",", "6", ",", "1"}], "]"}]], "Input",
 CellChangeTimes->{{3.646760163668606*^9, 3.646760166946803*^9}, 
   3.6467608366972713`*^9, {3.6572922098750553`*^9, 3.657292258156515*^9}, {
   3.657292423013629*^9, 3.657292425165235*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "[", 
  RowBox[{"2", ",", "6", ",", "2"}], "]"}]], "Input",
 CellChangeTimes->{{3.646760163668606*^9, 3.646760166946803*^9}, 
   3.6467608366972713`*^9, {3.6572922098750553`*^9, 3.657292266627708*^9}, {
   3.657292428645054*^9, 3.65729243104463*^9}}],

Cell["Take a column from the whole dataset", "Text",
 CellChangeTimes->{{3.65729229740871*^9, 3.657292303271203*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "[", 
  RowBox[{"All", ",", "\"\<EditionCount\>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.6572923127459383`*^9, 3.6572923536041727`*^9}, {
  3.6572924340926247`*^9, 3.6572924370361547`*^9}}],

Cell["\<\
Apply a function to a column. Here we total the number of different editions \
for all twenty works\
\>", "Text",
 CellChangeTimes->{{3.65729235977934*^9, 3.6572923784975643`*^9}}],

Cell[BoxData[
 RowBox[{"lyellResults", "[", 
  RowBox[{"Total", ",", "\"\<EditionCount\>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.6572923127459383`*^9, 3.6572923536041727`*^9}, {
  3.657292385698081*^9, 3.657292398775671*^9}}]
}, Open  ]]
