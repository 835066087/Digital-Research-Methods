Cell["Corpus: Some detective novels", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6528912224484377`*^9, 3.6528912318231697`*^9}, {3.652894652827334*^9, 
  3.652894655337706*^9}, {3.652896920896419*^9, 3.652896921950879*^9}, {
  3.652900319148108*^9, 3.652900338018691*^9}}],

Cell["\<\
For our sample corpus, we are going to batch download six detective novels by \
R. Austin Freeman\
\>", "Text",
 CellChangeTimes->{{3.6528912345270033`*^9, 3.65289127936537*^9}, {
  3.652900331052511*^9, 3.652900334834982*^9}}],

Cell[BoxData[
 RowBox[{"WikipediaSearch", "[", "\"\<r. austin freeman\>\"", "]"}]], "Input",\

 CellChangeTimes->{{3.652891137739032*^9, 3.652891202584354*^9}}],

Cell[BoxData[
 RowBox[{"freemanIDs", "=", 
  RowBox[{"Rest", "[", 
   RowBox[{"StringTrim", "[", 
    RowBox[{
     RowBox[{"StringSplit", "[", 
      RowBox[{"URLFetch", "@", 
       RowBox[{"URLBuild", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
          "\"\<http://archive.org/\>\"", ",", "\"\<advancedsearch.php\>\""}], 
          "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
           "\"\<q\>\"", "\[Rule]", " ", 
            "\"\<creator:\\\"Freeman, R. Austin (Richard Austin), 1862-1943\\\
\" AND collection:Gutenberg\>\""}], ",", 
           RowBox[{"\"\<fl[]\>\"", "\[Rule]", "\"\<identifier\>\""}], ",", 
           RowBox[{"\"\<rows\>\"", "\[Rule]", "50"}], ",", 
           RowBox[{"\"\<page\>\"", "\[Rule]", "1"}], ",", 
           RowBox[{"\"\<output\>\"", "\[Rule]", "\"\<csv\>\""}]}], "}"}]}], 
        "]"}]}], "]"}], ",", "\"\<\\\"\>\""}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.64511905737632*^9, 3.645119100463616*^9}, {
   3.645121872456016*^9, 3.645121898573406*^9}, {3.652900408721231*^9, 
   3.652900444767776*^9}, 3.652900557886491*^9}],

Cell["\<\
Now I am going to create a special directory to hold the files I want to \
download.\
\>", "Text",
 CellChangeTimes->{{3.652892666955104*^9, 3.652892689261303*^9}, 
   3.652900278133938*^9}],

Cell[BoxData[
 RowBox[{"Clear", "[", "localDownloadFilePath", "]"}]], "Input",
 CellChangeTimes->{{3.652901086137063*^9, 3.652901091437723*^9}}],

Cell[TextData[StyleBox["Make sure to change the following so that it is a \
valid file path on your own computer",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.6528996132446327`*^9, 3.652899662803152*^9}},
 Background->RGBColor[1, 0.85, 0.85]],

Cell[BoxData[
 RowBox[{"localDownloadFilePath", "=", 
  "\"\</Users/wjt/Downloads/freeman\>\""}]], "Input",
 CellChangeTimes->{{3.65289958930519*^9, 3.6528996279964657`*^9}, {
  3.6529010319866867`*^9, 3.65290103275397*^9}}],

Cell[BoxData[
 RowBox[{"If", "[", 
  RowBox[{
   RowBox[{"Not", "[", 
    RowBox[{"DirectoryQ", "[", "localDownloadFilePath", "]"}], "]"}], ",", 
   RowBox[{"CreateDirectory", "[", "localDownloadFilePath", "]"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.645119735915062*^9, 3.645119790154065*^9}, {
  3.652892699098353*^9, 3.652892704849229*^9}, {3.6529002467679663`*^9, 
  3.652900292852487*^9}, {3.6529005933946533`*^9, 3.652900594873671*^9}, {
  3.652901036809847*^9, 3.652901040009478*^9}}],

Cell["I am going to set this directory to be my default", "Text",
 CellChangeTimes->{{3.6528930434925547`*^9, 3.652893069543335*^9}, {
  3.6529007101104183`*^9, 3.652900719732915*^9}, {3.652901252074649*^9, 
  3.652901255545529*^9}}],

Cell[BoxData[
 RowBox[{"SetDirectory", "[", "localDownloadFilePath", "]"}]], "Input",
 CellChangeTimes->{{3.652893055269574*^9, 3.652893060574758*^9}, {
   3.6529007224692583`*^9, 3.6529007290448103`*^9}, 3.6529010564330187`*^9}],

Cell["Here is the URL for the text file of the first novel", "Text",
 CellChangeTimes->{{3.652900617049313*^9, 3.652900667766954*^9}}],

Cell[BoxData[
 RowBox[{"internetArchiveIDToURL", "[", 
  RowBox[{
  "freemanIDs", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.65290065020765*^9, 3.6529006586473713`*^9}}],

