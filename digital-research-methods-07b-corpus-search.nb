Cell[CellGroupData[{

Cell["Indexing words", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6529705305794163`*^9, 3.652970534097356*^9}}],

Cell["\<\
The next function takes the association of texts that we just created, and \
the name of a file, and it creates an index for that file.\
\>", "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.6529705482097607`*^9, 3.6529705506088943`*^9}, {3.652970651431678*^9, 
  3.652970691036968*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"indexWords", "[", 
   RowBox[{"fileassoc_", ",", "filename_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"ReplaceAll", "[", 
   RowBox[{
    RowBox[{"GroupBy", "[", 
     RowBox[{
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{"Thread", "[", 
         RowBox[{"List", "[", 
          RowBox[{
           RowBox[{"fileassoc", "[", "filename", "]"}], ",", 
           RowBox[{"Range", "@", 
            RowBox[{"Length", "[", 
             RowBox[{"fileassoc", "[", "filename", "]"}], "]"}]}]}], "]"}], 
         "]"}], ",", 
        RowBox[{
         RowBox[{"nonStopwordQ", "[", 
          RowBox[{"#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
           "]"}], "&"}]}], "]"}], ",", 
      RowBox[{"First", "\[Rule]", "Last"}]}], "]"}], ",", 
    RowBox[{"x_List", "\[Rule]", 
     RowBox[{"(", 
      RowBox[{"filename", "\[Rule]", "x"}], ")"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.645440348129765*^9, 3.645440358049274*^9}, {
  3.645440411639266*^9, 3.645440417670896*^9}, {3.6454404688568172`*^9, 
  3.645440535995023*^9}}],

Cell["\<\
We can try running it once so that we can see what kind of output it produces\
\
\>", "Text",
 CellChangeTimes->{{3.652970723020648*^9, 3.652970737243628*^9}}],

Cell[BoxData[
 RowBox[{"viewData", "[", 
  RowBox[{"indexWords", "[", 
   RowBox[{"freemanTexts", ",", "\"\<johnthorndykesca13882gut.txt\>\""}], 
   "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.652970749370727*^9, 3.65297078142052*^9}}],

Cell[TextData[{
 "If we scroll down a bit in the listing, we see that the word \
\[OpenCurlyDoubleQuote]professor\[CloseCurlyDoubleQuote] appears at word \
position 128. Since our association ",
 StyleBox["freemanTexts",
  FontSlant->"Italic"],
 " contains word lists, we can see the context for this instance of the word \
with the following command."
}], "Text",
 CellChangeTimes->{{3.6529708591449337`*^9, 3.6529709509657927`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
  "freemanTexts", "\[LeftDoubleBracket]", 
   "\"\<johnthorndykesca13882gut.txt\>\"", "\[RightDoubleBracket]"}], 
  "\[LeftDoubleBracket]", 
  RowBox[{
   RowBox[{"128", "-", "6"}], ";;", 
   RowBox[{"128", "+", "6"}]}], "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.652970952916888*^9, 3.6529710492510853`*^9}}],

Cell["The next instance is at word position 456", "Text",
 CellChangeTimes->{{3.652971057018743*^9, 3.65297106923438*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
  "freemanTexts", "\[LeftDoubleBracket]", 
   "\"\<johnthorndykesca13882gut.txt\>\"", "\[RightDoubleBracket]"}], 
  "\[LeftDoubleBracket]", 
  RowBox[{
   RowBox[{"456", "-", "6"}], ";;", 
   RowBox[{"456", "+", "6"}]}], "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.652970952916888*^9, 3.6529710492510853`*^9}, {
  3.652971080163011*^9, 3.652971083010034*^9}}]
}, Open  ]]
