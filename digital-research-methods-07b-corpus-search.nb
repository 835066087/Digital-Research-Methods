Cell[CellGroupData[{

Cell["An association containing word frequencies", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
   3.65297138313048*^9, 3.652971386041567*^9}, {3.6529726992918253`*^9, 
   3.652972707690872*^9}, 3.65297531591089*^9, {3.65297693142593*^9, 
   3.652976931624341*^9}}],

Cell[TextData[{
 "We can also create an association containing word frequencies for each \
text. Once again we ",
 StyleBox["Put",
  FontWeight->"Bold"],
 " a copy on our drive so we can ",
 StyleBox["Get",
  FontWeight->"Bold"],
 " it later."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.652975526142796*^9, 3.652975569901535*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
    "FileExistsQ", "[", "\"\<freemanWordFrequencies.assoc.m\>\"", "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"freemanWordFrequencies", "=", 
     RowBox[{"Get", "[", "\"\<freemanWordFrequencies.assoc.m\>\"", "]"}]}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"freemanWordFrequencies", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"MapThread", "[", 
        RowBox[{"Rule", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Keys", "[", "freemanTexts", "]"}], ",", 
           RowBox[{"Map", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Sort", "[", 
               RowBox[{
                RowBox[{"Tally", "[", 
                 RowBox[{"DeleteStopwords", "[", 
                  RowBox[{
                  "freemanTexts", "\[LeftDoubleBracket]", "#", 
                   "\[RightDoubleBracket]"}], "]"}], "]"}], ",", 
                RowBox[{
                 RowBox[{
                  RowBox[{
                  "#1", "\[LeftDoubleBracket]", "2", 
                   "\[RightDoubleBracket]"}], ">", 
                  RowBox[{
                  "#2", "\[LeftDoubleBracket]", "2", 
                   "\[RightDoubleBracket]"}]}], "&"}]}], "]"}], "&"}], ",", 
             RowBox[{"Keys", "[", "freemanTexts", "]"}]}], "]"}]}], "}"}]}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Put", "[", 
      RowBox[{
      "freemanWordFrequencies", ",", 
       "\"\<freemanWordFrequencies.assoc.m\>\""}], "]"}]}]}], "]"}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.6529742783880663`*^9, 3.652974400069559*^9}, {
  3.652974684172217*^9, 3.652974700674573*^9}, {3.6529754345630198`*^9, 
  3.652975522535383*^9}}],

Cell["Look up frequency of a single term in one text", "Text",
 CellChangeTimes->{{3.652974820846384*^9, 3.652974842133737*^9}}],

Cell[BoxData[
 RowBox[{"Cases", "[", 
  RowBox[{
   RowBox[{
   "freemanWordFrequencies", "\[LeftDoubleBracket]", 
    "\"\<theeyeofosiris24201gut.txt\>\"", "\[RightDoubleBracket]"}], ",", 
   RowBox[{"{", 
    RowBox[{"\"\<thorndyke\>\"", ",", "_"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.6529747376567907`*^9, 3.652974809270924*^9}}],

Cell["\<\
Function to look up frequency of a single term across all texts\
\>", "Text",
 CellChangeTimes->{{3.652974845189489*^9, 3.6529748522533617`*^9}, {
  3.652974940299337*^9, 3.652974941802431*^9}, {3.652975586972868*^9, 
  3.652975619283886*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"corpusWordFrequencies", "[", 
   RowBox[{"freqassoc_", ",", "keyword_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"TableForm", "[", 
   RowBox[{"Map", "[", 
    RowBox[{"Flatten", ",", 
     RowBox[{"MapThread", "[", 
      RowBox[{"List", ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Keys", "[", "freqassoc", "]"}], ",", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Cases", "[", 
             RowBox[{
              RowBox[{
              "freqassoc", "\[LeftDoubleBracket]", "#", 
               "\[RightDoubleBracket]"}], ",", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"keyword", ",", "n_"}], "}"}], "\[Rule]", "n"}]}], 
             "]"}], "&"}], ",", 
           RowBox[{"Keys", "[", "freqassoc", "]"}]}], "]"}]}], "}"}]}], 
      "]"}]}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6529747376567907`*^9, 3.652974809270924*^9}, {
  3.65297488493327*^9, 3.6529749055325413`*^9}, {3.652974960786686*^9, 
  3.652975030815662*^9}, {3.65297506245541*^9, 3.6529752910716543`*^9}}],

Cell[BoxData[
 RowBox[{"corpusWordFrequencies", "[", 
  RowBox[{"freemanWordFrequencies", ",", "\"\<thorndyke\>\""}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.65297503586383*^9, 3.6529750441668043`*^9}}],

Cell[BoxData[
 RowBox[{"corpusWordFrequencies", "[", 
  RowBox[{"freemanWordFrequencies", ",", "\"\<body\>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.65297503586383*^9, 3.6529750441668043`*^9}, {
  3.652975775735536*^9, 3.652975776567524*^9}, {3.6529758635729237`*^9, 
