Cell[CellGroupData[{

Cell["An association of texts", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.652895186103984*^9, 3.652895189494267*^9}}],

Cell[TextData[{
 "If our corpus is small enough, we can load all of the texts that we want to \
analyze into memory. We will store them in an association where the ",
 StyleBox["key",
  FontSlant->"Italic"],
 " is the file name of the text, and the ",
 StyleBox["value",
  FontSlant->"Italic"],
 " is the lowercased list of words in the order they appear in the text."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.652895193334201*^9, 3.6528953078339453`*^9}}],

Cell["\<\
Here is what one entry in the association is going to look like\
\>", "Text",
 CellChangeTimes->{{3.652895312153577*^9, 3.652895314801525*^9}, {
  3.652895348313559*^9, 3.652895357776004*^9}}],

Cell[BoxData[
 RowBox[{"Short", "[", 
  RowBox[{
   RowBox[{"\"\<johnthorndykesca13882gut.txt\>\"", "\[Rule]", 
    RowBox[{"ToLowerCase", "[", 
     RowBox[{"TextWords", "[", 
      RowBox[{"Import", "[", "\"\<johnthorndykesca13882gut.txt\>\"", "]"}], 
      "]"}], "]"}]}], ",", "10"}], "]"}]], "Input",
 CellChangeTimes->{{3.652895369026121*^9, 3.652895411806182*^9}, {
  3.652970424534562*^9, 3.652970431109313*^9}}],

Cell[TextData[{
 "Since it takes a while to create this association, we will store a copy of \
it. The first time the code below is run, the association file doesn\
\[CloseCurlyQuote]t exist so it is created from scratch. If we rerun the \
code, however, the ",
 StyleBox["Get",
  FontWeight->"Bold"],
 " command will be used to retrieve the association. If we want or need to \
recreate the association, we simply delete the local files from our drive."
}], "Text",
 CellChangeTimes->{{3.652895661253388*^9, 3.652895774736845*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"FileExistsQ", "[", "\"\<freemanTexts.assoc.m\>\"", "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"freemanTexts", "=", 
     RowBox[{"Get", "[", "\"\<freemanTexts.assoc.m\>\"", "]"}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"freemanTexts", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "\[Rule]", 
           RowBox[{"ToLowerCase", "@", 
            RowBox[{"TextWords", "@", 
             RowBox[{"Import", "@", "#"}]}]}]}], "&"}], ",", 
         RowBox[{"FileNames", "[", "\"\<*.txt\>\"", "]"}]}], "]"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Put", "[", 
      RowBox[{"freemanTexts", ",", "\"\<freemanTexts.assoc.m\>\""}], 
      "]"}]}]}], "]"}], ";"}]], "Input",
 CellChangeTimes->{{3.6454395458273087`*^9, 3.6454396537151957`*^9}, {
  3.6457151137729073`*^9, 3.645715183331461*^9}, {3.645715241975335*^9, 
  3.6457152738329487`*^9}, {3.6528957891772013`*^9, 3.652895856119014*^9}}],

Cell[BoxData[
 RowBox[{"FileNames", "[", "]"}]], "Input",
 CellChangeTimes->{{3.6528958466478243`*^9, 3.652895848494894*^9}}],

Cell["Number of texts", "Text",
 CellChangeTimes->{{3.652895879885477*^9, 3.652895881732819*^9}}],

Cell[BoxData[
 RowBox[{"Length", "[", "freemanTexts", "]"}]], "Input",
 CellChangeTimes->{{3.6528958844701557`*^9, 3.6528958890534363`*^9}}],

Cell["Number of words in each text", "Text",
 CellChangeTimes->{{3.652895891356921*^9, 3.652895895188271*^9}}],

Cell[BoxData[
 RowBox[{"Map", "[", 
  RowBox[{"Length", ",", 
   RowBox[{"Values", "[", "freemanTexts", "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.6528958990859423`*^9, 3.652895910148769*^9}}]
}, Open  ]]
