     RowBox[{"Scrollbars", "\[Rule]", "True"}]}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.511539182255096*^9, 3.51153920239167*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"lifespan", "[", "person_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Labeled", "[", 
   RowBox[{
    RowBox[{"Interval", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"PersonData", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Interpreter", "[", "\"\<ComputedPerson\>\"", "]"}], "[", 
          "person", "]"}], ",", "\"\<BirthDate\>\""}], "]"}], ",", 
       RowBox[{"PersonData", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Interpreter", "[", "\"\<ComputedPerson\>\"", "]"}], "[", 
          "person", "]"}], ",", "\"\<DeathDate\>\""}], "]"}]}], "}"}], "]"}], 
    ",", "person"}], "]"}]}]], "Input",
