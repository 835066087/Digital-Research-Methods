 RowBox[{
  RowBox[{"histevents", "=", 
   RowBox[{"EntityValue", "[", 
    RowBox[{
     RowBox[{"EntityValue", "[", 
      RowBox[{"\"\<HistoricalEvent\>\"", ",", "\"\<Entities\>\""}], "]"}], 
     ",", 
     RowBox[{"{", 
      RowBox[{"\"\<Name\>\"", ",", "\"\<StartDate\>\""}], "}"}], ",", 
     "\"\<EntityAssociation\>\""}], "]"}]}], ";"}]], "Input"],

Cell[CellGroupData[{

Cell["The early 1970s", "Subsection",
 CellChangeTimes->{{3.683816369114716*^9, 3.683816371649979*^9}}],

Cell[TextData[{
 "Suppose we wanted to find the events relating to the early 1970s. We ",
 StyleBox["Select",
  FontWeight->"Bold"],
 " those events whose starting date is between 1970 and 1971"
}], "Text",
 CellChangeTimes->{{3.651757116235545*^9, 3.65175717693676*^9}, {
   3.651757335124186*^9, 3.6517573356432858`*^9}, {3.6838155297294617`*^9, 
   3.683815561418095*^9}, 3.683815593552657*^9, {3.6838162663038673`*^9, 
   3.683816291405257*^9}}],

Cell[BoxData[
 RowBox[{"viewData", "[", 
  RowBox[{"Keys", "[", 
   RowBox[{"Select", "[", 
    RowBox[{"histevents", ",", 
     RowBox[{
      RowBox[{"1970", "\[LessEqual]", 
       RowBox[{
        RowBox[{"DateList", "[", 
         RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
         "]"}], "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
       "\[LessEqual]", "1971"}], "&"}]}], "]"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.641755046859334*^9, 3.6417551745854816`*^9}, {
   3.6517594973373632`*^9, 3.651759498199438*^9}, 3.68381558801278*^9, {
   3.683815784339731*^9, 3.683815815601858*^9}, {3.683816300229706*^9, 
   3.683816300620905*^9}}]
}, Open  ]],

