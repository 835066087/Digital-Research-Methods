Cell[CellGroupData[{

Cell["Matching terms in two lists", "Subsubsection",
 CellChangeTimes->{{3.683299235294631*^9, 3.683299248365378*^9}}],

Cell[TextData[{
 "The ",
 StyleBox["listMatchGraph",
  FontSlant->"Italic"],
 " function takes two lists of terms in order of descending frequency and \
shows the relative positions of terms that appear in both lists."
}], "Text",
 CellChangeTimes->{{3.648101570839045*^9, 3.648101617005406*^9}, {
   3.648101776801227*^9, 3.648101837310381*^9}, 3.683299406624626*^9, 
   3.6832996653077993`*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"listMatchGraph", "[", 
   RowBox[{"left_", ",", "right_", ",", "lbl_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "idxl", ",", "idxr", ",", "leftidx", ",", "rightidx", ",", "lself", ",", 
      "rself", ",", "lcoords", ",", "rcoords", ",", "gr"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"idxl", "=", 
      RowBox[{"ToString", "/@", 
       RowBox[{"Range", "[", 
        RowBox[{"Length", "[", "left", "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"idxr", "=", 
      RowBox[{"ToString", "/@", 
       RowBox[{"Range", "[", 
        RowBox[{"Length", "[", "right", "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"leftidx", "=", 
      RowBox[{"StringRiffle", "/@", 
       RowBox[{"Thread", "[", 
        RowBox[{"List", "[", 
         RowBox[{"idxl", ",", "left"}], "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rightidx", "=", 
      RowBox[{"StringRiffle", "/@", 
       RowBox[{"Thread", "[", 
        RowBox[{"List", "[", 
         RowBox[{"idxr", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"StringJoin", "[", 
             RowBox[{"#", ",", "\"\< \>\""}], "]"}], "&"}], "/@", "right"}]}],
          "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"lself", "=", 
      RowBox[{"Thread", "[", 
       RowBox[{"Rule", "[", 
        RowBox[{"leftidx", ",", "leftidx"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rself", "=", 
      RowBox[{"Thread", "[", 
       RowBox[{"Rule", "[", 
        RowBox[{"rightidx", ",", "rightidx"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"lcoords", "=", 
      RowBox[{"Thread", "[", 
       RowBox[{"Rule", "[", 
        RowBox[{"leftidx", ",", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "8"}], ",", "#"}], "}"}], "&"}], ",", 
           RowBox[{"-", 
            RowBox[{"Range", "[", 
             RowBox[{"Length", "[", "leftidx", "]"}], "]"}]}]}], "]"}]}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"rcoords", "=", "\[IndentingNewLine]", 
      RowBox[{"Thread", "[", 
       RowBox[{"Rule", "[", 
        RowBox[{"rightidx", ",", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"8", ",", "#"}], "}"}], "&"}], ",", 
           RowBox[{"-", 
            RowBox[{"Range", "[", 
             RowBox[{"Length", "[", "rightidx", "]"}], "]"}]}]}], "]"}]}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"gr", "=", 
      RowBox[{"DeleteCases", "[", 
       RowBox[{
        RowBox[{"Thread", "[", 
         RowBox[{"Rule", "[", 
          RowBox[{"leftidx", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"rightidx", "\[LeftDoubleBracket]", 
              RowBox[{"First", "@", 
               RowBox[{"First", "@", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"Position", "[", 
                   RowBox[{"right", ",", "#"}], "]"}], "/.", 
                  RowBox[{
                   RowBox[{"{", "}"}], "\[Rule]", 
                   RowBox[{"{", 
                    RowBox[{"{", "0", "}"}], "}"}]}]}], ")"}]}]}], 
              "\[RightDoubleBracket]"}], "&"}], "/@", "left"}]}], "]"}], 
         "]"}], ",", 
        RowBox[{"_", "\[Rule]", "List"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"GraphPlot", "[", 
        RowBox[{
         RowBox[{"Join", "[", 
          RowBox[{"lself", ",", "rself", ",", "gr"}], "]"}], ",", 
         RowBox[{"VertexLabeling", "\[Rule]", "True"}], ",", 
         RowBox[{"VertexCoordinateRules", "\[Rule]", 
          RowBox[{"Join", "[", 
           RowBox[{"lcoords", ",", "rcoords"}], "]"}]}], ",", 
         RowBox[{"SelfLoopStyle", "\[Rule]", "None"}], ",", 
         RowBox[{"EdgeRenderingFunction", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"Gray", ",", " ", 
              RowBox[{"Arrow", "[", 
               RowBox[{"#1", "+", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{"1.6", ",", "0"}], "}"}], ",", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{"-", "1.6"}], ",", "0"}], "}"}]}], "}"}]}], 
               "]"}]}], "}"}], " ", "&"}], " ", ")"}]}], ",", 
         RowBox[{"VertexRenderingFunction", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"Black", ",", 
              RowBox[{"Text", "[", 
               RowBox[{"#2", ",", "#1"}], "]"}]}], "}"}], "&"}], ")"}]}], ",", 
         RowBox[{"ImageSize", "\[Rule]", "Large"}]}], "]"}], ",", 
       RowBox[{"PlotLabel", "\[Rule]", "lbl"}], ",", 
       RowBox[{"LabelStyle", "\[Rule]", 
        RowBox[{"{", "12", "}"}]}]}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.64309991541346*^9, 3.64310009939907*^9}, {
   3.643100142077235*^9, 3.6431002055143948`*^9}, {3.643127733223009*^9, 
